                'color': '#32CD32',
                'applications': '–ù–µ–π—Ä–æ–∏–º–ø–ª–∞–Ω—Ç–∞—Ç—ã, –∏—Å–∫—É—Å—Å—Ç–≤–µ–Ω–Ω—ã–µ –æ—Ä–≥–∞–Ω—ã',
                'discovery': '–ì–∞—Ä–≤–∞—Ä–¥—Å–∫–∞—è –º–µ–¥–∏—Ü–∏–Ω—Å–∫–∞—è —à–∫–æ–ª–∞ 2032',
                'features': ['—Ä–æ—Å—Ç —Å —Ç–∫–∞–Ω—è–º–∏', '–∞–Ω—Ç–∏–±–∞–∫—Ç–µ—Ä–∏–∞–ª—å–Ω—ã–π', '–±–∏–æ—Ä–∞–∑–ª–∞–≥–∞–µ–º—ã–π']
            },
        ]
        
