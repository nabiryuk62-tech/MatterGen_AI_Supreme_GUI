                'rarity': '‚≠ê‚≠ê‚≠ê‚≠ê‚≠ê‚≠ê',
                'color': '#FF1493',
                'applications': '–ò–æ–Ω–Ω—ã–µ –¥–≤–∏–≥–∞—Ç–µ–ª–∏, –∫–≤–∞–Ω—Ç–æ–≤—ã–µ –∫–æ–º–ø—å—é—Ç–µ—Ä—ã',
                'discovery': '–ú–∞—Å—Å–∞—á—É—Å–µ—Ç—Å–∫–∏–π —Ç–µ—Ö–Ω–æ–ª–æ–≥–∏—á–µ—Å–∫–∏–π –∏–Ω—Å—Ç–∏—Ç—É—Ç 2028',
                'features': ['—Å–≤–µ—Ä—Ö–ø—Ä–æ–≤–æ–¥–∏–º–æ—Å—Ç—å', '–ø–∞–º—è—Ç—å —Ñ–æ—Ä–º—ã', '—Å–∞–º–æ–æ—Ä–≥–∞–Ω–∏–∑–∞—Ü–∏—è']
            },
        ]
        
        # –ú–ï–î–ò–¶–ò–ù–°–ö–ò–ï –ú–ê–¢–ï–†–ò–ê–õ–´
        medical_materials = [
            {
                'id': 'BIO-TITAN',
                'name': '‚ù§Ô∏è –ë–∏–æ—Ç–∏—Ç–∞–Ω Living Alloy',
