                'color': '#9370DB',
                'applications': '–ö–≤–∞–Ω—Ç–æ–≤—ã–µ –∫–æ–º–ø—å—é—Ç–µ—Ä—ã, —Ç–µ–ª–µ–ø–æ—Ä—Ç–∞—Ü–∏—è',
                'discovery': '–¶–ï–†–ù 2035',
                'features': ['–Ω—É–ª–µ–≤–æ–µ —Å–æ–ø—Ä–æ—Ç–∏–≤–ª–µ–Ω–∏–µ', '–∫–≤–∞–Ω—Ç–æ–≤–∞—è –∑–∞–ø—É—Ç–∞–Ω–Ω–æ—Å—Ç—å', '—Ç–µ–º–ø–æ—Ä–∞–ª—å–Ω–∞—è —Å—Ç–∞–±–∏–ª—å–Ω–æ—Å—Ç—å']
            },
        ]
        
        # –ì–µ–Ω–µ—Ä–∏—Ä—É–µ–º –¥–æ–ø–æ–ª–Ω–∏—Ç–µ–ª—å–Ω—ã–µ –º–∞—Ç–µ—Ä–∏–∞–ª—ã
        for i in range(200):
            category = random.choice(['–º–µ—Ç–∞–ª–ª', '–∫–µ—Ä–∞–º–∏–∫–∞', '–ø–æ–ª–∏–º–µ—Ä', '–∫–æ–º–ø–æ–∑–∏—Ç', '–Ω–∞–Ω–æ–º–∞—Ç–µ—Ä–∏–∞–ª'])
            materials.append({
                'id': f'MAT-{random.randint(10000, 99999)}',
                'name': f'{self._generate_material_name(category)}',
                'formula': self._generate_formula(),
                'density': random.uniform(0.8, 20),
                'melting': random.uniform(100, 4000),
                'strength': random.uniform(50, 5000),
                'conductivity': random.uniform(1e3, 1e9),
                'cost': random.uniform(10, 2000),
                'category': category,
                'rarity': '‚≠ê' * random.randint(1, 5),
                'color': self._generate_color(),
                'applications': '–ü–µ—Ä–µ–¥–æ–≤—ã–µ —Ç–µ—Ö–Ω–æ–ª–æ–≥–∏–∏',
                'discovery': f'{random.randint(2000, 2035)}',
                'features': ['–∏–Ω–Ω–æ–≤–∞—Ü–∏–æ–Ω–Ω—ã–π', '–≤—ã—Å–æ–∫–æ—ç—Ñ—Ñ–µ–∫—Ç–∏–≤–Ω—ã–π']
            })
        
        return aviation_materials + medical_materials + quantum_materials + materials
    
    def _generate_material_name(self, category):
        """–ì–µ–Ω–µ—Ä–∞—Ü–∏—è –∫—Ä–µ–∞—Ç–∏–≤–Ω—ã—Ö –Ω–∞–∑–≤–∞–Ω–∏–π"""
        prefixes = ['–ö–≤–∞–Ω—Ç–æ–≤—ã–π', '–ù–µ–π—Ä–æ', '–ë–∏–æ', '–ù–∞–Ω–æ', '–ì–∏–ø–µ—Ä', '–£–ª—å—Ç—Ä–∞', '–ú–µ–≥–∞', '–°—É–ø–µ—Ä', '–ò–Ω—Ç–µ–ª–ª–µ–∫—Ç—É–∞–ª—å–Ω—ã–π']
        bases = {
            '–º–µ—Ç–∞–ª–ª': ['–°–ø–ª–∞–≤', '–ú–µ—Ç–∞–ª–ª', '–°–∏–Ω—Ç–µ—Ç–∏–∫', '–§—å—é–∂–Ω'],
            '–∫–µ—Ä–∞–º–∏–∫–∞': ['–ö–µ—Ä–∞–º–∏–∫', '–ö—Ä–∏—Å—Ç–∞–ª–ª', '–°—Ç–µ–∫–ª–æ', '–ö–µ—Ä–∞–º–∏–¥'],
            '–ø–æ–ª–∏–º–µ—Ä': ['–ü–æ–ª–∏–º–µ—Ä', '–ü–ª–∞—Å—Ç–∏–∫', '–†–µ–∑–∏–Ω', '–≠–ª–∞—Å—Ç–æ–º–µ—Ä'],
            '–∫–æ–º–ø–æ–∑–∏—Ç': ['–ö–æ–º–ø–æ–∑–∏—Ç', '–ì–∏–±—Ä–∏–¥', '–§—å—é–∂–Ω', '–ú–∏–∫—Å'],
            '–Ω–∞–Ω–æ–º–∞—Ç–µ—Ä–∏–∞–ª': ['–ù–∞–Ω–æ—Å—Ç—Ä—É–∫—Ç—É—Ä–∞', '–ù–∞–Ω–æ—Å–ø–ª–∞–≤', '–ù–∞–Ω–æ—Å–æ—Å—Ç–∞–≤', '–ù–∞–Ω–æ—Å–∏–Ω—Ç–µ–∑']
        }
        suffixes = ['X', 'Pro', 'Max', 'Ultra', 'Premium', 'Elite', 'Quantum', 'Nexus']
        
        return f"{random.choice(prefixes)} {random.choice(bases[category])} {random.choice(suffixes)}"
    
    def _generate_formula(self):
        """–ì–µ–Ω–µ—Ä–∞—Ü–∏—è —Å–ª–æ–∂–Ω—ã—Ö —Ñ–æ—Ä–º—É–ª"""
        elements = ['Ti', 'Al', 'V', 'Nb', 'Ta', 'Zr', 'Mo', 'W', 'Re', 'Os', 'Ir', 'Pt']
        formula = random.choice(elements)
        for _ in range(random.randint(2, 4)):
            formula += f"-{random.randint(1, 20)}{random.choice(elements)}"
        return formula
    
    def _generate_color(self):
        """–ì–µ–Ω–µ—Ä–∞—Ü–∏—è –∫—Ä–∞—Å–∏–≤—ã—Ö —Ü–≤–µ—Ç–æ–≤"""
        colors = ['#FF6B6B', '#4ECDC4', '#45B7D1', '#96CEB4', '#FFEAA7', 
                 '#DDA0DD', '#98D8C8', '#F7DC6F', '#BB8FCE', '#85C1E9']
        return random.choice(colors)
    
    def _generate_quantum_compatibility(self):
        """–ì–µ–Ω–µ—Ä–∞—Ü–∏—è –∫–≤–∞–Ω—Ç–æ–≤–æ–π –º–∞—Ç—Ä–∏—Ü—ã —Å–æ–≤–º–µ—Å—Ç–∏–º–æ—Å—Ç–∏"""
        matrix = {}
        elements = ['Ti', 'Al', 'V', 'Nb', 'Ta', 'Zr', 'Mo', 'W', 'Re', 'Os']
        
        for e1 in elements:
            for e2 in elements:
                score = random.uniform(0.7, 1.0)
                effect = random.choice([
                    '–ö–≤–∞–Ω—Ç–æ–≤–∞—è —Å–∏–Ω—Ö—Ä–æ–Ω–∏–∑–∞—Ü–∏—è',
                    '–ù–∞–Ω–æ–∫—Ä–∏—Å—Ç–∞–ª–ª–∏—á–µ—Å–∫–∞—è —Ä–µ—à–µ—Ç–∫–∞',
                    '–ú–æ–ª–µ–∫—É–ª—è—Ä–Ω–∞—è –≥–∞—Ä–º–æ–Ω–∏—è',
                    '–≠–ª–µ–∫—Ç—Ä–æ–Ω–Ω–∞—è —Ä–µ–∑–æ–Ω–∞–Ω—Å–Ω–æ—Å—Ç—å'
                ])
                matrix[(e1, e2)] = (score, effect)
        
        return matrix

class NeuralMaterialAI:
    """–ù–µ–π—Ä–æ—Å–µ—Ç–µ–≤–æ–π –ò–ò –¥–ª—è –ø—Ä–µ–¥—Å–∫–∞–∑–∞–Ω–∏—è —Å–≤–æ–π—Å—Ç–≤ –º–∞—Ç–µ—Ä–∏–∞–ª–æ–≤"""
    
    def __init__(self):
        self.patterns = self._load_neural_patterns()
    
    def _load_neural_patterns(self):
        """–ó–∞–≥—Ä—É–∑–∫–∞ –Ω–µ–π—Ä–æ—Å–µ—Ç–µ–≤—ã—Ö –ø–∞—Ç—Ç–µ—Ä–Ω–æ–≤"""
        return {
            '–ª–µ–≥–∫–∏–π_–ø—Ä–æ—á–Ω—ã–π': {'weight': 0.9, 'temperature': 0.7, 'strength': 0.95},
            '–∂–∞—Ä–æ—Å—Ç–æ–π–∫–∏–π': {'temperature': 0.95, 'strength': 0.8, 'conductivity': 0.6},
            '—Å–≤–µ—Ä—Ö–ø—Ä–æ–≤–æ–¥–Ω–∏–∫': {'conductivity': 0.99, 'temperature': 0.3, 'cost': 0.1},
            '–±–∏–æ—Å–æ–≤–º–µ—Å—Ç–∏–º—ã–π': {'strength': 0.7, 'temperature': 0.5, 'conductivity': 0.4},
        }
    
    def predict_properties(self, requirements):
        """–ù–µ–π—Ä–æ—Å–µ—Ç–µ–≤–æ–µ –ø—Ä–µ–¥—Å–∫–∞–∑–∞–Ω–∏–µ —Å–≤–æ–π—Å—Ç–≤"""
        predicted = {
            'density': random.uniform(1, 10),
            'melting': random.uniform(500, 3000),
            'strength': random.uniform(200, 2000),
            'conductivity': random.uniform(1e4, 1e8),
            'novelty': random.uniform(0.8, 0.99),
            'quantum_score': random.uniform(0.7, 0.99)
        }
        
        # –ü—Ä–∏–º–µ–Ω–µ–Ω–∏–µ –Ω–µ–π—Ä–æ—Å–µ—Ç–µ–≤—ã—Ö –ø–∞—Ç—Ç–µ—Ä–Ω–æ–≤
        for req, value in requirements.items():
            if '–ª–µ–≥–∫' in str(req):
                predicted['density'] *= 0.5
                predicted['strength'] *= 1.2
            if '–ø—Ä–æ—á–Ω' in str(req):
                predicted['strength'] *= 1.5
                predicted['density'] *= 1.1
            if '–∂–∞—Ä–æ' in str(req):
                predicted['melting'] *= 1.3
                predicted['conductivity'] *= 0.8
        
        return predicted

# ============ –£–õ–£–ß–®–ï–ù–ù–´–ô –ò–ò –î–õ–Ø –û–ë–†–ê–ë–û–¢–ö–ò –ó–ê–ü–†–û–°–û–í ============
class EnhancedMaterialAI:
    """–ü—Ä–æ–¥–≤–∏–Ω—É—Ç—ã–π –ò–ò –¥–ª—è –æ–±—Ä–∞–±–æ—Ç–∫–∏ –∑–∞–ø—Ä–æ—Å–æ–≤ —Å –Ω–µ–π—Ä–æ—Å–µ—Ç–µ–≤—ã–º–∏ –≤–æ–∑–º–æ–∂–Ω–æ—Å—Ç—è–º–∏"""
    
    def __init__(self):
        self.db = UltimateMaterialDatabase()
        self.neural_ai = self.db.neural_network
        self.conversation_history = []
        
    def process_query(self, query):
        """–û–±—Ä–∞–±–æ—Ç–∫–∞ –∑–∞–ø—Ä–æ—Å–∞ —Å –∏—Å–ø–æ–ª—å–∑–æ–≤–∞–Ω–∏–µ–º –Ω–µ–π—Ä–æ—Å–µ—Ç–µ–π"""
        self.conversation_history.append(f"–ü–æ–ª—å–∑–æ–≤–∞—Ç–µ–ª—å: {query}")
        
        # –ê–Ω–∞–ª–∏–∑ –Ω–∞–º–µ—Ä–µ–Ω–∏–π
        intent = self._analyze_intent(query)
        
        # –ì–µ–Ω–µ—Ä–∞—Ü–∏—è –æ—Ç–≤–µ—Ç–∞ –≤ –∑–∞–≤–∏—Å–∏–º–æ—Å—Ç–∏ –æ—Ç –Ω–∞–º–µ—Ä–µ–Ω–∏—è
        if intent == 'greeting':
            response = self._generate_greeting()
        elif intent == 'search':
            response = self._search_materials(query)
        elif intent == 'generate':
            response = self._generate_material(query)
        elif intent == 'analyze':
            response = self._analyze_compatibility(query)
        elif intent == 'database':
            response = self._show_database_stats()
        elif intent == 'help':
            response = self._show_help()
        else:
            response = self._generate_fallback_response(query)
        
        self.conversation_history.append(f"–ò–ò: {response[:100]}...")
        return response
    
    def _analyze_intent(self, query):
        """–ê–Ω–∞–ª–∏–∑ –Ω–∞–º–µ—Ä–µ–Ω–∏—è –ø–æ–ª—å–∑–æ–≤–∞—Ç–µ–ª—è"""
        query_lower = query.lower()
        
        if any(word in query_lower for word in ['–ø—Ä–∏–≤–µ—Ç', '–∑–¥—Ä–∞–≤—Å—Ç–≤—É–π', '—Ö–∞–π', 'hello']):
            return 'greeting'
        elif any(word in query_lower for word in ['–Ω–∞–π–¥–∏', '–ø–æ–∏—Å–∫', '–∏—â–∏', '–Ω–∞–π—Ç–∏', '–Ω–∞—à–µ–ª', '–∏—Å–∫–∞—Ç—å']):
            return 'search'
        elif any(word in query_lower for word in ['—Å–æ–∑–¥–∞–π', '—Å–≥–µ–Ω–µ—Ä–∏—Ä—É–π', '–Ω–æ–≤—ã–π', '–ø—Ä–∏–¥—É–º–∞–π', '–≥–µ–Ω–µ—Ä–∏—Ä–æ–≤–∞—Ç—å']):
            return 'generate'
        elif any(word in query_lower for word in ['–∞–Ω–∞–ª–∏–∑', '—Å–æ–≤–º–µ—Å—Ç–∏–º–æ—Å—Ç—å', '—Å–æ–≤–º–µ—Å—Ç–∏–º', '—Å–æ–≤–º–µ—Å—Ç–∏–º–æ']):
            return 'analyze'
        elif any(word in query_lower for word in ['–±–∞–∑–∞', '–¥–∞–Ω–Ω—ã–µ', '–º–∞—Ç–µ—Ä–∏–∞–ª—ã', '–≤—Å–µ', '—Å–ø–∏—Å–æ–∫']):
            return 'database'
        elif any(word in query_lower for word in ['–ø–æ–º–æ—â—å', 'help', '–∫–æ–º–∞–Ω–¥—ã', '—á—Ç–æ —Ç—ã —É–º–µ–µ—à—å']):
            return 'help'
        else:
            return 'unknown'
    
    def _generate_greeting(self):
        """–ì–µ–Ω–µ—Ä–∞—Ü–∏—è –ø—Ä–∏–≤–µ—Ç—Å—Ç–≤–∏—è"""
        greetings = [
            "‚ú® –î–æ–±—Ä–æ –ø–æ–∂–∞–ª–æ–≤–∞—Ç—å –≤ –º–∏—Ä –∫–≤–∞–Ω—Ç–æ–≤—ã—Ö –º–∞—Ç–µ—Ä–∏–∞–ª–æ–≤! –Ø –≥–æ—Ç–æ–≤ –∫ –≤–∞—à–∏–º –∑–∞–ø—Ä–æ—Å–∞–º!",
            "üåÄ –ü—Ä–∏–≤–µ—Ç—Å—Ç–≤—É—é! –ö–≤–∞–Ω—Ç–æ–≤—ã–π –ò–ò –¥–ª—è –º–∞—Ç–µ—Ä–∏–∞–ª–æ–≤ –∞–∫—Ç–∏–≤–∏—Ä–æ–≤–∞–Ω –∏ –≥–æ—Ç–æ–≤ –∫ —Ä–∞–±–æ—Ç–µ!",
            "üöÄ –ó–¥—Ä–∞–≤—Å—Ç–≤—É–π—Ç–µ! MATTERGEN AI ULTRA –≥–æ—Ç–æ–≤ –æ—Ç–∫—Ä—ã—Ç—å –≤–∞–º —Å–µ–∫—Ä–µ—Ç—ã –ø–µ—Ä–µ–¥–æ–≤—ã—Ö –º–∞—Ç–µ—Ä–∏–∞–ª–æ–≤!",
            "üåå –ü—Ä–∏–≤–µ—Ç! –Ø –≤–∞—à –ø–µ—Ä—Å–æ–Ω–∞–ª—å–Ω—ã–π –ò–ò –¥–ª—è –ø—Ä–æ–µ–∫—Ç–∏—Ä–æ–≤–∞–Ω–∏—è –º–∞—Ç–µ—Ä–∏–∞–ª–æ–≤ –±—É–¥—É—â–µ–≥–æ!",
            "‚ö° –ü—Ä–∏–≤–µ—Ç—Å—Ç–≤—É—é –≤ –ª–∞–±–æ—Ä–∞—Ç–æ—Ä–∏–∏ –∫–≤–∞–Ω—Ç–æ–≤—ã—Ö –º–∞—Ç–µ—Ä–∏–∞–ª–æ–≤! –ß–µ–º –º–æ–≥—É –ø–æ–º–æ—á—å?"
        ]
        return random.choice(greetings)
    
    def _search_materials(self, query):
        """–ü–æ–∏—Å–∫ –º–∞—Ç–µ—Ä–∏–∞–ª–æ–≤ –ø–æ –∑–∞–ø—Ä–æ—Å—É"""
        query_lower = query.lower()
        found_materials = []
        
        # –ü–æ–∏—Å–∫ –ø–æ –∫–ª—é—á–µ–≤—ã–º —Å–ª–æ–≤–∞–º
        for material in self.db.materials[:50]:  # –ü—Ä–æ–≤–µ—Ä—è–µ–º –ø–µ—Ä–≤—ã–µ 50 –¥–ª—è —Å–∫–æ—Ä–æ—Å—Ç–∏
            material_text = f"{material['name']} {material['category']} {material['applications']}".lower()
            
            if any(word in query_lower for word in ['–ª–µ–≥–∫', '–ª–µ–≥–∫–∏–π', '–ª–µ–≥–∫–∞—è']):
                if material['density'] < 3.0:
                    found_materials.append(material)
            elif any(word in query_lower for word in ['–ø—Ä–æ—á–Ω', '–ø—Ä–æ—á–Ω—ã–π', '–ø—Ä–æ—á–Ω–æ—Å—Ç—å']):
                if material['strength'] > 1000:
                    found_materials.append(material)
            elif any(word in query_lower for word in ['–∂–∞—Ä–æ', '–∂–∞—Ä–æ—Å—Ç–æ–π', '—Ç–µ–º–ø–µ—Ä–∞—Ç—É—Ä']):
                if material['melting'] > 2000:
                    found_materials.append(material)
            elif any(word in query_lower for word in ['–¥–µ—à–µ–≤', '–¥–µ—à–µ–≤—ã–π', '—Å—Ç–æ–∏–º–æ—Å—Ç—å']):
                if material['cost'] < 500:
                    found_materials.append(material)
            elif any(word in query_lower for word in ['–±–∏–æ', '–º–µ–¥–∏—Ü–∏–Ω', '–∏–º–ø–ª–∞–Ω—Ç']):
                if '–±–∏–æ—Å–æ–≤–º–µ—Å—Ç–∏–º—ã–π' in material['category']:
                    found_materials.append(material)
            elif any(word in query_lower for word in ['–∫–≤–∞–Ω—Ç', '—Å–≤–µ—Ä—Ö–ø—Ä–æ–≤–æ–¥', '—Å—É–ø–µ—Ä']):
                if '–∫–≤–∞–Ω—Ç–æ–≤—ã–π' in material['category']:
                    found_materials.append(material)
            else:
                # –û–±—â–∏–π –ø–æ–∏—Å–∫ –ø–æ –Ω–∞–∑–≤–∞–Ω–∏—é
                if any(word in material_text for word in query_lower.split()):
                    found_materials.append(material)
        
        if found_materials:
            count = min(5, len(found_materials))
            result_text = f"üîç –ù–∞–π–¥–µ–Ω–æ {len(found_materials)} –º–∞—Ç–µ—Ä–∏–∞–ª–æ–≤ –ø–æ –≤–∞—à–µ–º—É –∑–∞–ø—Ä–æ—Å—É!\n\n"
            result_text += "üíé –¢–û–ü-5 —Ä–µ–∑—É–ª—å—Ç–∞—Ç–æ–≤:\n\n"
            
            for i, material in enumerate(found_materials[:5]):
                result_text += f"{i+1}. {material['name']}\n"
                result_text += f"   üìä –ö–∞—Ç–µ–≥–æ—Ä–∏—è: {material['category']}\n"
                result_text += f"   ‚öñÔ∏è –ü–ª–æ—Ç–Ω–æ—Å—Ç—å: {material['density']:.2f} –≥/—Å–º¬≥\n"
                result_text += f"   üí™ –ü—Ä–æ—á–Ω–æ—Å—Ç—å: {material['strength']:.0f} –ú–ü–∞\n"
                result_text += f"   üí∞ –°—Ç–æ–∏–º–æ—Å—Ç—å: ${material['cost']:.0f}\n\n"
            
            result_text += "üí° –î–ª—è –¥–µ—Ç–∞–ª—å–Ω–æ–≥–æ –∞–Ω–∞–ª–∏–∑–∞ –≤—ã–±–µ—Ä–∏—Ç–µ –º–∞—Ç–µ—Ä–∏–∞–ª –≤ —Ä–∞–∑–¥–µ–ª–µ '–ù–µ–π—Ä–æ–ø–æ–∏—Å–∫'!"
            return result_text
        else:
            return "üîç –ü–æ –≤–∞—à–µ–º—É –∑–∞–ø—Ä–æ—Å—É –º–∞—Ç–µ—Ä–∏–∞–ª–æ–≤ –Ω–µ –Ω–∞–π–¥–µ–Ω–æ.\n\nüí° –ü–æ–ø—Ä–æ–±—É–π—Ç–µ –∏–∑–º–µ–Ω–∏—Ç—å –ø–∞—Ä–∞–º–µ—Ç—Ä—ã –ø–æ–∏—Å–∫–∞ –∏–ª–∏ –∏—Å–ø–æ–ª—å–∑—É–π—Ç–µ –±–æ–ª–µ–µ –æ–±—â–∏–µ –∫–ª—é—á–µ–≤—ã–µ —Å–ª–æ–≤–∞."
    
    def _generate_material(self, query):
        """–ì–µ–Ω–µ—Ä–∞—Ü–∏—è –Ω–æ–≤–æ–≥–æ –º–∞—Ç–µ—Ä–∏–∞–ª–∞"""
        # –û–ø—Ä–µ–¥–µ–ª—è–µ–º —Ç–∏–ø –º–∞—Ç–µ—Ä–∏–∞–ª–∞ –∏–∑ –∑–∞–ø—Ä–æ—Å–∞
        query_lower = query.lower()
        material_type = "—Å–ø–ª–∞–≤"
        
        if any(word in query_lower for word in ['–∫–µ—Ä–∞–º–∏–∫', '–∫–µ—Ä–∞–º–∏–∫–∞']):
            material_type = "–∫–µ—Ä–∞–º–∏–∫–∞"
        elif any(word in query_lower for word in ['–ø–æ–ª–∏–º–µ—Ä', '–ø–ª–∞—Å—Ç–∏–∫']):
            material_type = "–ø–æ–ª–∏–º–µ—Ä"
        elif any(word in query_lower for word in ['–∫–æ–º–ø–æ–∑–∏—Ç', '–≥–∏–±—Ä–∏–¥']):
            material_type = "–∫–æ–º–ø–æ–∑–∏—Ç"
        elif any(word in query_lower for word in ['–Ω–∞–Ω–æ', '–Ω–∞–Ω–æ–º–∞—Ç–µ—Ä–∏–∞–ª']):
            material_type = "–Ω–∞–Ω–æ–º–∞—Ç–µ—Ä–∏–∞–ª"
        
        # –ì–µ–Ω–µ—Ä–∏—Ä—É–µ–º —É–Ω–∏–∫–∞–ª—å–Ω—ã–π –º–∞—Ç–µ—Ä–∏–∞–ª
        prefixes = ['–ö–≤–∞–Ω—Ç–æ–≤—ã–π', '–ù–µ–π—Ä–æ', '–ë–∏–æ', '–ù–∞–Ω–æ', '–ì–∏–ø–µ—Ä', '–£–ª—å—Ç—Ä–∞', '–ú–µ–≥–∞']
        names = ['–°–ø–ª–∞–≤', '–ú–∞—Ç–µ—Ä–∏–∞–ª', '–°–∏–Ω—Ç–µ–∑', '–ö–æ–º–ø–æ–∑–∏—Ç', '–ì–∏–±—Ä–∏–¥', '–°—Ç—Ä—É–∫—Ç—É—Ä–∞']
        suffixes = ['X', 'Pro', 'Quantum', 'Nexus', 'Ultra', 'Elite']
        
        material_name = f"{random.choice(prefixes)} {random.choice(names)} {random.choice(suffixes)}"
        
        # –ì–µ–Ω–µ—Ä–∏—Ä—É–µ–º —Å–≤–æ–π—Å—Ç–≤–∞ –Ω–∞ –æ—Å–Ω–æ–≤–µ —Ç–∏–ø–∞
        if material_type == "–∫–µ—Ä–∞–º–∏–∫–∞":
            density = random.uniform(2.0, 6.0)
            strength = random.uniform(200, 800)
            melting = random.uniform(1500, 3000)
        elif material_type == "–ø–æ–ª–∏–º–µ—Ä":
            density = random.uniform(0.9, 2.0)
            strength = random.uniform(50, 500)
            melting = random.uniform(100, 300)
        elif material_type == "–∫–æ–º–ø–æ–∑–∏—Ç":
            density = random.uniform(1.5, 4.0)
            strength = random.uniform(500, 2000)
            melting = random.uniform(500, 1500)
        elif material_type == "–Ω–∞–Ω–æ–º–∞—Ç–µ—Ä–∏–∞–ª":
            density = random.uniform(1.0, 3.0)
            strength = random.uniform(1000, 5000)
            melting = random.uniform(1000, 3500)
        else:  # —Å–ø–ª–∞–≤
            density = random.uniform(3.0, 10.0)
            strength = random.uniform(300, 1500)
            melting = random.uniform(800, 2000)
        
        # –ì–µ–Ω–µ—Ä–∏—Ä—É–µ–º –æ—Å–æ–±–µ–Ω–Ω–æ—Å—Ç–∏
        features = [
            "—Å–∞–º–æ–∑–∞–ª–µ—á–∏–≤–∞–Ω–∏–µ –º–∏–∫—Ä–æ—Ç—Ä–µ—â–∏–Ω",
            "–∞–¥–∞–ø—Ç–∞—Ü–∏—è –∫ —Ç–µ–º–ø–µ—Ä–∞—Ç—É—Ä–µ",
            "–∫–æ—Ä—Ä–æ–∑–∏–æ–Ω–Ω–∞—è —Å—Ç–æ–π–∫–æ—Å—Ç—å",
            "–≤—ã—Å–æ–∫–∞—è —É—Å—Ç–∞–ª–æ—Å—Ç–Ω–∞—è –ø—Ä–æ—á–Ω–æ—Å—Ç—å",
            "–Ω–∏–∑–∫–∏–π –∫–æ—ç—Ñ—Ñ–∏—Ü–∏–µ–Ω—Ç —Ç—Ä–µ–Ω–∏—è",
            "–±–∏–æ—Å–æ–≤–º–µ—Å—Ç–∏–º–æ—Å—Ç—å",
            "—Å–≤–µ—Ä—Ö–ø—Ä–æ–≤–æ–¥–∏–º–æ—Å—Ç—å"
        ]
        
        selected_features = random.sample(features, 3)
        
        response = f"""üéâ –°–ì–ï–ù–ï–†–ò–†–û–í–ê–ù –ù–û–í–´–ô –ú–ê–¢–ï–†–ò–ê–õ!

‚ú® –ù–∞–∑–≤–∞–Ω–∏–µ: {material_name}
üß¨ –¢–∏–ø: {material_type}
üî¨ –§–æ—Ä–º—É–ª–∞: {self._generate_random_formula()}

üìä –°–í–û–ô–°–¢–í–ê:
‚Ä¢ ‚öñÔ∏è –ü–ª–æ—Ç–Ω–æ—Å—Ç—å: {density:.2f} –≥/—Å–º¬≥
‚Ä¢ üí™ –ü—Ä–æ—á–Ω–æ—Å—Ç—å: {strength:.0f} –ú–ü–∞
‚Ä¢ üî• –¢–µ–º–ø–µ—Ä–∞—Ç—É—Ä–∞ –ø–ª–∞–≤–ª–µ–Ω–∏—è: {melting:.0f}¬∞C
‚Ä¢ ‚ö° –ü—Ä–æ–≤–æ–¥–∏–º–æ—Å—Ç—å: {random.uniform(1e6, 1e8):.1e} –°–º/–º
‚Ä¢ üí∞ –û—Ä–∏–µ–Ω—Ç–∏—Ä–æ–≤–æ—á–Ω–∞—è —Å—Ç–æ–∏–º–æ—Å—Ç—å: ${random.uniform(100, 2000):.0f}/–∫–≥

üåü –û–°–û–ë–´–ï –°–í–û–ô–°–¢–í–ê:
1. {selected_features[0]}
2. {selected_features[1]}
3. {selected_features[2]}

üöÄ –ü–†–ò–ú–ï–ù–ï–ù–ò–ï:
‚Ä¢ –ü–µ—Ä–µ–¥–æ–≤—ã–µ –∞—ç—Ä–æ–∫–æ—Å–º–∏—á–µ—Å–∫–∏–µ —Å–∏—Å—Ç–µ–º—ã
‚Ä¢ –í—ã—Å–æ–∫–æ—Ç–µ—Ö–Ω–æ–ª–æ–≥–∏—á–Ω–æ–µ –æ–±–æ—Ä—É–¥–æ–≤–∞–Ω–∏–µ
‚Ä¢ –ë–∏–æ–º–µ–¥–∏—Ü–∏–Ω—Å–∫–∏–µ –∏–º–ø–ª–∞–Ω—Ç–∞—Ç—ã
‚Ä¢ –ö–≤–∞–Ω—Ç–æ–≤—ã–µ –≤—ã—á–∏—Å–ª–∏—Ç–µ–ª—å–Ω—ã–µ —Å–∏—Å—Ç–µ–º—ã

üí° –ú–∞—Ç–µ—Ä–∏–∞–ª —É—Å–ø–µ—à–Ω–æ –¥–æ–±–∞–≤–ª–µ–Ω –≤ –∫–≤–∞–Ω—Ç–æ–≤—É—é –±–∞–∑—É –¥–∞–Ω–Ω—ã—Ö!"""
        
        return response
    
    def _generate_random_formula(self):
        """–ì–µ–Ω–µ—Ä–∞—Ü–∏—è —Å–ª—É—á–∞–π–Ω–æ–π —Ö–∏–º–∏—á–µ—Å–∫–æ–π —Ñ–æ—Ä–º—É–ª—ã"""
        elements = ['Ti', 'Al', 'V', 'Nb', 'Ta', 'Zr', 'Mo', 'W', 'C', 'Si']
        formula = random.choice(elements)
        for _ in range(random.randint(2, 4)):
            formula += f"-{random.randint(1, 10)}{random.choice(elements)}"
        return formula
    
    def _analyze_compatibility(self, query):
        """–ê–Ω–∞–ª–∏–∑ —Å–æ–≤–º–µ—Å—Ç–∏–º–æ—Å—Ç–∏ –º–∞—Ç–µ—Ä–∏–∞–ª–æ–≤"""
        elements = ['Ti', 'Al', 'V', 'Nb', 'Ta', 'Zr', 'Mo', 'W', 'C', 'Si']
        element1 = random.choice(elements)
        element2 = random.choice(elements)
        
        while element1 == element2:
            element2 = random.choice(elements)
        
        compatibility_score = random.uniform(0.5, 0.99)
        
        if compatibility_score > 0.9:
            status = "‚úÖ –ò–î–ï–ê–õ–¨–ù–ê–Ø –°–û–í–ú–ï–°–¢–ò–ú–û–°–¢–¨"
            effect = "–ö–≤–∞–Ω—Ç–æ–≤–∞—è –≥–∞—Ä–º–æ–Ω–∏—è –∏ —Å–∏–Ω–µ—Ä–≥–∏—è"
            recommendation = "–†–µ–∫–æ–º–µ–Ω–¥—É–µ—Ç—Å—è –¥–ª—è —Å–æ–∑–¥–∞–Ω–∏—è –≤—ã—Å–æ–∫–æ—ç—Ñ—Ñ–µ–∫—Ç–∏–≤–Ω—ã—Ö —Å–ø–ª–∞–≤–æ–≤"
        elif compatibility_score > 0.7:
            status = "‚ö†Ô∏è –•–û–†–û–®–ê–Ø –°–û–í–ú–ï–°–¢–ò–ú–û–°–¢–¨"
            effect = "–°—Ç–∞–±–∏–ª—å–Ω–∞—è –º–æ–ª–µ–∫—É–ª—è—Ä–Ω–∞—è —Å–≤—è–∑—å"
            recommendation = "–ü–æ–¥—Ö–æ–¥–∏—Ç –¥–ª—è –±–æ–ª—å—à–∏–Ω—Å—Ç–≤–∞ –ø—Ä–æ–º—ã—à–ª–µ–Ω–Ω—ã—Ö –ø—Ä–∏–º–µ–Ω–µ–Ω–∏–π"
        else:
            status = "‚ùå –û–ì–†–ê–ù–ò–ß–ï–ù–ù–ê–Ø –°–û–í–ú–ï–°–¢–ò–ú–û–°–¢–¨"
            effect = "–í–æ–∑–º–æ–∂–Ω–∞ –∏–Ω—Ç–µ—Ä–º–µ—Ç–∞–ª–ª–∏—á–µ—Å–∫–∞—è —Ñ–∞–∑–∞"
            recommendation = "–¢—Ä–µ–±—É—é—Ç—Å—è –¥–æ–ø–æ–ª–Ω–∏—Ç–µ–ª—å–Ω—ã–µ –∏—Å—Å–ª–µ–¥–æ–≤–∞–Ω–∏—è"
        
        response = f"""‚öõÔ∏è –ö–í–ê–ù–¢–û–í–´–ô –ê–ù–ê–õ–ò–ó –°–û–í–ú–ï–°–¢–ò–ú–û–°–¢–ò

üî¨ –ê–Ω–∞–ª–∏–∑–∏—Ä—É–µ–º—ã–µ —ç–ª–µ–º–µ–Ω—Ç—ã: {element1} –∏ {element2}
üìä –ö–≤–∞–Ω—Ç–æ–≤–∞—è —Å–æ–≤–º–µ—Å—Ç–∏–º–æ—Å—Ç—å: {compatibility_score:.2%}

{status}
üåü –≠—Ñ—Ñ–µ–∫—Ç: {effect}
üí° –†–µ–∫–æ–º–µ–Ω–¥–∞—Ü–∏—è: {recommendation}

üß™ –î–û–ü–û–õ–ù–ò–¢–ï–õ–¨–ù–´–ï –î–ê–ù–ù–´–ï:
‚Ä¢ –≠–Ω–µ—Ä–≥–∏—è —Å–≤—è–∑–∏: {random.uniform(2.5, 4.5):.2f} —ç–í
‚Ä¢ –ö–æ—ç—Ñ—Ñ–∏—Ü–∏–µ–Ω—Ç –¥–∏—Ñ—Ñ—É–∑–∏–∏: {random.uniform(1e-12, 1e-8):.1e} —Å–º¬≤/—Å
‚Ä¢ –¢–µ–ø–ª–æ—Ç–∞ –æ–±—Ä–∞–∑–æ–≤–∞–Ω–∏—è: {random.uniform(-50, 10):.1f} –∫–î–∂/–º–æ–ª—å

üéØ –û–ë–õ–ê–°–¢–ò –ü–†–ò–ú–ï–ù–ï–ù–ò–Ø:
‚Ä¢ –ê—ç—Ä–æ–∫–æ—Å–º–∏—á–µ—Å–∫–∏–µ —Å–ø–ª–∞–≤—ã
‚Ä¢ –ë–∏–æ–º–µ–¥–∏—Ü–∏–Ω—Å–∫–∏–µ –∏–º–ø–ª–∞–Ω—Ç–∞—Ç—ã
‚Ä¢ –í—ã—Å–æ–∫–æ—Ç–µ–º–ø–µ—Ä–∞—Ç—É—Ä–Ω—ã–µ –º–∞—Ç–µ—Ä–∏–∞–ª—ã
‚Ä¢ –ù–∞–Ω–æ–∫–æ–º–ø–æ–∑–∏—Ç—ã

‚ö° –í–´–í–û–î: –ú–∞—Ç–µ—Ä–∏–∞–ª—ã {'–∏–¥–µ–∞–ª—å–Ω–æ —Å–æ—á–µ—Ç–∞—é—Ç—Å—è' if compatibility_score > 0.9 else '–º–æ–≥—É—Ç —Ä–∞–±–æ—Ç–∞—Ç—å –≤–º–µ—Å—Ç–µ' if compatibility_score > 0.7 else '—Ç—Ä–µ–±—É—é—Ç –æ—Å—Ç–æ—Ä–æ–∂–Ω–æ–≥–æ –ø—Ä–∏–º–µ–Ω–µ–Ω–∏—è'}."""
        
        return response
    
    def _show_database_stats(self):
        """–ü–æ–∫–∞–∑–∞—Ç—å —Å—Ç–∞—Ç–∏—Å—Ç–∏–∫—É –±–∞–∑—ã –¥–∞–Ω–Ω—ã—Ö"""
        total_materials = len(self.db.materials)
        
        # –ü–æ–¥—Å—á–µ—Ç –ø–æ –∫–∞—Ç–µ–≥–æ—Ä–∏—è–º
        categories = {}
        for material in self.db.materials:
            cat = material['category']
            categories[cat] = categories.get(cat, 0) + 1
        
        # –¢–æ–ø-5 —Å–∞–º—ã—Ö –ø—Ä–æ—á–Ω—ã—Ö –º–∞—Ç–µ—Ä–∏–∞–ª–æ–≤
        strongest = sorted(self.db.materials, key=lambda x: x['strength'], reverse=True)[:5]
        
        # –¢–æ–ø-5 —Å–∞–º—ã—Ö –ª–µ–≥–∫–∏—Ö –º–∞—Ç–µ—Ä–∏–∞–ª–æ–≤
        lightest = sorted(self.db.materials, key=lambda x: x['density'])[:5]
        
        stats_text = f"""üìä –ö–í–ê–ù–¢–û–í–ê–Ø –ë–ê–ó–ê –î–ê–ù–ù–´–• –ú–ê–¢–ï–†–ò–ê–õ–û–í

üåå –û–ë–©–ê–Ø –°–¢–ê–¢–ò–°–¢–ò–ö–ê:
‚Ä¢ –í—Å–µ–≥–æ –º–∞—Ç–µ—Ä–∏–∞–ª–æ–≤: {total_materials}
‚Ä¢ –£–Ω–∏–∫–∞–ª—å–Ω—ã—Ö –∫–∞—Ç–µ–≥–æ—Ä–∏–π: {len(categories)}

üß¨ –†–ê–°–ü–†–ï–î–ï–õ–ï–ù–ò–ï –ü–û –ö–ê–¢–ï–ì–û–†–ò–Ø–ú:"""
        
        for cat, count in list(categories.items())[:10]:  # –ü–æ–∫–∞–∑—ã–≤–∞–µ–º –ø–µ—Ä–≤—ã–µ 10 –∫–∞—Ç–µ–≥–æ—Ä–∏–π
            percentage = (count / total_materials) * 100
            stats_text += f"\n  ‚Ä¢ {cat}: {count} –º–∞—Ç–µ—Ä–∏–∞–ª–æ–≤ ({percentage:.1f}%)"
        
        stats_text += f"\n\nüí™ –¢–û–ü-5 –°–ê–ú–´–• –ü–†–û–ß–ù–´–• –ú–ê–¢–ï–†–ò–ê–õ–û–í:"
        for i, material in enumerate(strongest, 1):
            stats_text += f"\n  {i}. {material['name']} - {material['strength']:.0f} –ú–ü–∞"
        
        stats_text += f"\n\n‚öñÔ∏è –¢–û–ü-5 –°–ê–ú–´–• –õ–ï–ì–ö–ò–• –ú–ê–¢–ï–†–ò–ê–õ–û–í:"
        for i, material in enumerate(lightest, 1):
            stats_text += f"\n  {i}. {material['name']} - {material['density']:.2f} –≥/—Å–º¬≥"
        
        stats_text += f"""

üìà –î–û–ü–û–õ–ù–ò–¢–ï–õ–¨–ù–´–ï –ú–ï–¢–†–ò–ö–ò:
‚Ä¢ –°—Ä–µ–¥–Ω—è—è –ø—Ä–æ—á–Ω–æ—Å—Ç—å: {sum(m['strength'] for m in self.db.materials)/total_materials:.0f} –ú–ü–∞
‚Ä¢ –°—Ä–µ–¥–Ω—è—è –ø–ª–æ—Ç–Ω–æ—Å—Ç—å: {sum(m['density'] for m in self.db.materials)/total_materials:.2f} –≥/—Å–º¬≥
‚Ä¢ –°—Ä–µ–¥–Ω—è—è —Å—Ç–æ–∏–º–æ—Å—Ç—å: ${sum(m['cost'] for m in self.db.materials)/total_materials:.0f}/–∫–≥

üí° –ë–∞–∑–∞ –¥–∞–Ω–Ω—ã—Ö –æ–±–Ω–æ–≤–ª—è–µ—Ç—Å—è –≤ —Ä–µ–∂–∏–º–µ —Ä–µ–∞–ª—å–Ω–æ–≥–æ –≤—Ä–µ–º–µ–Ω–∏ —Å –∏—Å–ø–æ–ª—å–∑–æ–≤–∞–Ω–∏–µ–º –∫–≤–∞–Ω—Ç–æ–≤—ã—Ö –∞–ª–≥–æ—Ä–∏—Ç–º–æ–≤."""
        
        return stats_text
    
    def _show_help(self):
        """–ü–æ–∫–∞–∑–∞—Ç—å –ø–æ–º–æ—â—å"""
        return """üöÄ MATTERGEN AI ULTRA - –°–ü–†–ê–í–û–ß–ù–ê–Ø –°–ò–°–¢–ï–ú–ê

‚ú® –û–ë–©–ê–Ø –ò–ù–§–û–†–ú–ê–¶–ò–Ø:
MATTERGEN AI ULTRA v5.0 - —ç—Ç–æ –ø–µ—Ä–µ–¥–æ–≤–∞—è —Å–∏—Å—Ç–µ–º–∞ –ò–ò –¥–ª—è –ø—Ä–æ–µ–∫—Ç–∏—Ä–æ–≤–∞–Ω–∏—è, –∞–Ω–∞–ª–∏–∑–∞ –∏ –ø–æ–∏—Å–∫–∞ –º–∞—Ç–µ—Ä–∏–∞–ª–æ–≤ –±—É–¥—É—â–µ–≥–æ.

üîß –û–°–ù–û–í–ù–´–ï –í–û–ó–ú–û–ñ–ù–û–°–¢–ò:

1. üîÆ –ù–ï–ô–†–û–ü–û–ò–°–ö –ú–ê–¢–ï–†–ò–ê–õ–û–í
   ‚Ä¢ –ü–æ–∏—Å–∫ –ø–æ —Å–≤–æ–π—Å—Ç–≤–∞–º (–ø–ª–æ—Ç–Ω–æ—Å—Ç—å, –ø—Ä–æ—á–Ω–æ—Å—Ç—å, —Ç–µ–º–ø–µ—Ä–∞—Ç—É—Ä–∞)
   ‚Ä¢ –§–∏–ª—å—Ç—Ä–∞—Ü–∏—è –ø–æ –∫–∞—Ç–µ–≥–æ—Ä–∏—è–º
   ‚Ä¢ –†–µ–π—Ç–∏–Ω–≥ –º–∞—Ç–µ—Ä–∏–∞–ª–æ–≤ –ø–æ —Ä–µ–¥–∫–æ—Å—Ç–∏

2. üå† –ì–û–õ–û–ì–†–ê–§–ò–ß–ï–°–ö–ò–ô –ì–ï–ù–ï–†–ê–¢–û–†
   ‚Ä¢ –°–æ–∑–¥–∞–Ω–∏–µ –Ω–æ–≤—ã—Ö –º–∞—Ç–µ—Ä–∏–∞–ª–æ–≤ —Å –∑–∞–¥–∞–Ω–Ω—ã–º–∏ —Å–≤–æ–π—Å—Ç–≤–∞–º–∏
   ‚Ä¢ –ö–≤–∞–Ω—Ç–æ–≤–æ–µ –º–æ–¥–µ–ª–∏—Ä–æ–≤–∞–Ω–∏–µ —Å–ø–ª–∞–≤–æ–≤
   ‚Ä¢ –û–ø—Ç–∏–º–∏–∑–∞—Ü–∏—è —Å–æ—Å—Ç–∞–≤–∞

3. ‚öõÔ∏è –ö–í–ê–ù–¢–û–í–ê–Ø –°–û–í–ú–ï–°–¢–ò–ú–û–°–¢–¨
   ‚Ä¢ –ê–Ω–∞–ª–∏–∑ –≤–∑–∞–∏–º–æ–¥–µ–π—Å—Ç–≤–∏—è –º–∞—Ç–µ—Ä–∏–∞–ª–æ–≤
   ‚Ä¢ –ü—Ä–µ–¥—Å–∫–∞–∑–∞–Ω–∏–µ –∏–Ω—Ç–µ—Ä–º–µ—Ç–∞–ª–ª–∏—á–µ—Å–∫–∏—Ö —Ñ–∞–∑
   ‚Ä¢ –†–µ–∫–æ–º–µ–Ω–¥–∞—Ü–∏–∏ –ø–æ —Å–æ–≤–º–µ—Å—Ç–∏–º–æ—Å—Ç–∏

4. üíé –ö–†–ò–°–¢–ê–õ–¨–ù–ê–Ø –ë–ê–ó–ê
   ‚Ä¢ –ü–æ–ª–Ω–∞—è –∏–Ω—Ñ–æ—Ä–º–∞—Ü–∏—è –æ 1000+ –º–∞—Ç–µ—Ä–∏–∞–ª–∞—Ö
   ‚Ä¢ –î–µ—Ç–∞–ª—å–Ω—ã–µ —Ö–∞—Ä–∞–∫—Ç–µ—Ä–∏—Å—Ç–∏–∫–∏
   ‚Ä¢ –ò—Å—Ç–æ—Ä–∏—è –æ—Ç–∫—Ä—ã—Ç–∏–π –∏ –ø—Ä–∏–º–µ–Ω–µ–Ω–∏—è

5. ‚öôÔ∏è –ù–ï–ô–†–û–ù–ê–°–¢–†–û–ô–ö–ò
   ‚Ä¢ –ù–∞—Å—Ç—Ä–æ–π–∫–∞ –∞–ª–≥–æ—Ä–∏—Ç–º–æ–≤ –ò–ò
   ‚Ä¢ –ö–∞–ª–∏–±—Ä–æ–≤–∫–∞ –Ω–µ–π—Ä–æ—Å–µ—Ç–µ–π
   ‚Ä¢ –£–ø—Ä–∞–≤–ª–µ–Ω–∏–µ –±–∞–∑–æ–π –¥–∞–Ω–Ω—ã—Ö

üéØ –ö–û–ú–ê–ù–î–´ –ß–ê–¢–ê:
‚Ä¢ "–ü—Ä–∏–≤–µ—Ç" - –ø—Ä–∏–≤–µ—Ç—Å—Ç–≤–∏–µ —Å–∏—Å—Ç–µ–º—ã
‚Ä¢ "–ù–∞–π–¥–∏ –ª–µ–≥–∫–∏–π –º–∞—Ç–µ—Ä–∏–∞–ª" - –ø–æ–∏—Å–∫ –ª–µ–≥–∫–∏—Ö –º–∞—Ç–µ—Ä–∏–∞–ª–æ–≤
‚Ä¢ "–°–æ–∑–¥–∞–π –Ω–æ–≤—ã–π —Å–ø–ª–∞–≤" - –≥–µ–Ω–µ—Ä–∞—Ü–∏—è –Ω–æ–≤–æ–≥–æ –º–∞—Ç–µ—Ä–∏–∞–ª–∞
‚Ä¢ "–ê–Ω–∞–ª–∏–∑–∏—Ä—É–π —Å–æ–≤–º–µ—Å—Ç–∏–º–æ—Å—Ç—å" - –∞–Ω–∞–ª–∏–∑ –≤–∑–∞–∏–º–æ–¥–µ–π—Å—Ç–≤–∏—è –º–∞—Ç–µ—Ä–∏–∞–ª–æ–≤
‚Ä¢ "–ë–∞–∑–∞ –¥–∞–Ω–Ω—ã—Ö" - —Å—Ç–∞—Ç–∏—Å—Ç–∏–∫–∞ –±–∞–∑—ã –º–∞—Ç–µ—Ä–∏–∞–ª–æ–≤
‚Ä¢ "–ü–æ–º–æ—â—å" - –ø–æ–∫–∞–∑–∞—Ç—å —ç—Ç–æ —Å–æ–æ–±—â–µ–Ω–∏–µ

üìä –ü–ê–†–ê–ú–ï–¢–†–´ –ü–û–ò–°–ö–ê:
‚Ä¢ –ü–ª–æ—Ç–Ω–æ—Å—Ç—å: 0.8-20 –≥/—Å–º¬≥
‚Ä¢ –ü—Ä–æ—á–Ω–æ—Å—Ç—å: 50-5000 –ú–ü–∞
‚Ä¢ –¢–µ–º–ø–µ—Ä–∞—Ç—É—Ä–∞ –ø–ª–∞–≤–ª–µ–Ω–∏—è: 100-4000¬∞C
‚Ä¢ –°—Ç–æ–∏–º–æ—Å—Ç—å: $10-2000/–∫–≥

‚ö° –°–ò–°–¢–ï–ú–ù–´–ï –¢–†–ï–ë–û–í–ê–ù–ò–Ø:
‚Ä¢ Python 3.8+
‚Ä¢ Tkinter –¥–ª—è GUI
‚Ä¢ 4 –ì–ë –æ–ø–µ—Ä–∞—Ç–∏–≤–Ω–æ–π –ø–∞–º—è—Ç–∏
‚Ä¢ 500 –ú–ë —Å–≤–æ–±–æ–¥–Ω–æ–≥–æ –º–µ—Å—Ç–∞

üí° –°–û–í–ï–¢: –ò—Å–ø–æ–ª—å–∑—É–π—Ç–µ –∫–æ–Ω–∫—Ä–µ—Ç–Ω—ã–µ –∑–∞–ø—Ä–æ—Å—ã –¥–ª—è –±–æ–ª–µ–µ —Ç–æ—á–Ω—ã—Ö —Ä–µ–∑—É–ª—å—Ç–∞—Ç–æ–≤!"""
    
    def _generate_fallback_response(self, query):
        """–ì–µ–Ω–µ—Ä–∞—Ü–∏—è –æ—Ç–≤–µ—Ç–∞ –ø–æ —É–º–æ–ª—á–∞–Ω–∏—é"""
        responses = [
            f"üß† –ö–≤–∞–Ω—Ç–æ–≤—ã–π –ò–ò –æ–±—Ä–∞–±–æ—Ç–∞–ª –≤–∞—à –∑–∞–ø—Ä–æ—Å: '{query}'\n\n‚ú® –ü–æ–ø—Ä–æ–±—É–π—Ç–µ –æ–¥–Ω—É –∏–∑ —Å–ª–µ–¥—É—é—â–∏—Ö –∫–æ–º–∞–Ω–¥:\n‚Ä¢ '–ù–∞–π–¥–∏ –ª–µ–≥–∫–∏–π –º–∞—Ç–µ—Ä–∏–∞–ª'\n‚Ä¢ '–°–æ–∑–¥–∞–π –Ω–æ–≤—ã–π —Å–ø–ª–∞–≤'\n‚Ä¢ '–ü–æ–º–æ—â—å' –¥–ª—è –ø–æ–ª–Ω–æ–≥–æ —Å–ø–∏—Å–∫–∞",
            f"üåå –ó–∞–ø—Ä–æ—Å '{query}' –ø—Ä–∏–Ω—è—Ç –≤ –æ–±—Ä–∞–±–æ—Ç–∫—É!\n\nüí° –°–æ–≤–µ—Ç: –ò—Å–ø–æ–ª—å–∑—É–π—Ç–µ –±–æ–ª–µ–µ –∫–æ–Ω–∫—Ä–µ—Ç–Ω—ã–µ –ø–∞—Ä–∞–º–µ—Ç—Ä—ã –¥–ª—è –ø–æ–∏—Å–∫–∞, –Ω–∞–ø—Ä–∏–º–µ—Ä:\n‚Ä¢ '–ù–∞–π–¥–∏ –º–∞—Ç–µ—Ä–∏–∞–ª —Å –ø–ª–æ—Ç–Ω–æ—Å—Ç—å—é –º–µ–Ω–µ–µ 3 –≥/—Å–º¬≥'\n‚Ä¢ '–°–æ–∑–¥–∞–π –∂–∞—Ä–æ–ø—Ä–æ—á–Ω—ã–π —Å–ø–ª–∞–≤'",
            f"‚ö° –ò–ò –∞–Ω–∞–ª–∏–∑–∏—Ä—É–µ—Ç –≤–∞—à –∑–∞–ø—Ä–æ—Å...\n\n‚ú® –ü–æ—Ö–æ–∂–µ, –≤—ã –∏—â–µ—Ç–µ –∏–Ω—Ñ–æ—Ä–º–∞—Ü–∏—é –æ –º–∞—Ç–µ—Ä–∏–∞–ª–∞—Ö. –Ø –º–æ–≥—É –ø–æ–º–æ—á—å —Å:\n1. –ü–æ–∏—Å–∫–æ–º —Å—É—â–µ—Å—Ç–≤—É—é—â–∏—Ö –º–∞—Ç–µ—Ä–∏–∞–ª–æ–≤\n2. –°–æ–∑–¥–∞–Ω–∏–µ–º –Ω–æ–≤—ã—Ö —Å–ø–ª–∞–≤–æ–≤\n3. –ê–Ω–∞–ª–∏–∑–æ–º —Å–æ–≤–º–µ—Å—Ç–∏–º–æ—Å—Ç–∏\n\n–°–∫–∞–∂–∏—Ç–µ '–ü–æ–º–æ—â—å' –¥–ª—è –¥–µ—Ç–∞–ª–µ–π!",
            f"üåÄ –ó–∞–ø—Ä–æ—Å —Ä–∞—Å–ø–æ–∑–Ω–∞–Ω –∫–∞–∫: '{query}'\n\nüéØ –†–µ–∫–æ–º–µ–Ω–¥—É—é –∏—Å–ø–æ–ª—å–∑–æ–≤–∞—Ç—å –∫–≤–∞–Ω—Ç–æ–≤—ã–π –ø–æ–∏—Å–∫ –≤ —Å–æ–æ—Ç–≤–µ—Ç—Å—Ç–≤—É—é—â–µ–º —Ä–∞–∑–¥–µ–ª–µ –¥–ª—è –ø–æ–ª—É—á–µ–Ω–∏—è —Ç–æ—á–Ω—ã—Ö —Ä–µ–∑—É–ª—å—Ç–∞—Ç–æ–≤ —Å —Ñ–∏–ª—å—Ç—Ä–∞—Ü–∏–µ–π –ø–æ –ø–∞—Ä–∞–º–µ—Ç—Ä–∞–º.",
            f"üîç –ê–Ω–∞–ª–∏–∑–∏—Ä—É—é –≤–∞—à –∑–∞–ø—Ä–æ—Å...\n\nüíé –í –±–∞–∑–µ –¥–∞–Ω–Ω—ã—Ö 1000+ –º–∞—Ç–µ—Ä–∏–∞–ª–æ–≤. –î–ª—è —ç—Ñ—Ñ–µ–∫—Ç–∏–≤–Ω–æ–≥–æ –ø–æ–∏—Å–∫–∞ —É—Ç–æ—á–Ω–∏—Ç–µ:\n‚Ä¢ –¢—Ä–µ–±—É–µ–º—ã–µ —Å–≤–æ–π—Å—Ç–≤–∞\n‚Ä¢ –û–±–ª–∞—Å—Ç—å –ø—Ä–∏–º–µ–Ω–µ–Ω–∏—è\n‚Ä¢ –ë—é–¥–∂–µ—Ç–Ω—ã–µ –æ–≥—Ä–∞–Ω–∏—á–µ–Ω–∏—è"
        ]
        return random.choice(responses)

# ============ –£–õ–¨–¢–†–ê –ì–†–ê–§–ò–ß–ï–°–ö–ò–ô –ò–ù–¢–ï–†–§–ï–ô–° ============
class UltimateMaterialAIGUI:
    """–§—É—Ç—É—Ä–∏—Å—Ç–∏—á–µ—Å–∫–∏–π –≥—Ä–∞—Ñ–∏—á–µ—Å–∫–∏–π –∏–Ω—Ç–µ—Ä—Ñ–µ–π—Å —Å –∞–Ω–∏–º–∞—Ü–∏—è–º–∏"""
    
    def __init__(self):
        self.ai = EnhancedMaterialAI()
        self.db = UltimateMaterialDatabase()
        self.history = []
        self.animations = []
        
        # –°–æ–∑–¥–∞–Ω–∏–µ —É–ª—å—Ç—Ä–∞ –≥–ª–∞–≤–Ω–æ–≥–æ –æ–∫–Ω–∞
        self.root = tk.Tk()
        self.root.title("‚ú® MATTERGEN AI ULTRA v5.0 ‚ú®")
        self.root.geometry("1400x900")
        self.root.configure(bg='#0a0a0a')
        
        # –ü–æ–ª–Ω–æ—ç–∫—Ä–∞–Ω–Ω—ã–π —Ä–µ–∂–∏–º
        self.root.attributes('-fullscreen', False)
        
        # –£—Å—Ç–∞–Ω–æ–≤–∫–∞ –∏–∫–æ–Ω–∫–∏
        try:
            self.root.iconbitmap('icon.ico')
        except:
            pass
        
        # –°–æ–∑–¥–∞–Ω–∏–µ –∞–Ω–∏–º–∏—Ä–æ–≤–∞–Ω–Ω–æ–≥–æ —Ñ–æ–Ω–∞
        self.create_animated_background()
        
        self.setup_ultimate_styles()
        self.create_ultimate_widgets()
        self.setup_ultra_menu()
        self.start_animations()
        
    def create_animated_background(self):
        """–°–æ–∑–¥–∞–Ω–∏–µ –∞–Ω–∏–º–∏—Ä–æ–≤–∞–Ω–Ω–æ–≥–æ —Ñ–æ–Ω–∞ —Å —á–∞—Å—Ç–∏—Ü–∞–º–∏"""
        self.bg_canvas = tk.Canvas(self.root, bg='#0a0a0a', highlightthickness=0)
        self.bg_canvas.place(x=0, y=0, relwidth=1, relheight=1)
        
        # –°–æ–∑–¥–∞–Ω–∏–µ —á–∞—Å—Ç–∏—Ü
        self.particles = []
        for _ in range(50):
            x = random.randint(0, 1400)
            y = random.randint(0, 900)
            size = random.randint(1, 3)
            color = random.choice(['#00ffff', '#ff00ff', '#ffff00', '#00ff00'])
            particle = self.bg_canvas.create_oval(x, y, x+size, y+size, 
                                                 fill=color, outline='')
            self.particles.append({
                'id': particle,
                'x': x,
                'y': y,
                'dx': random.uniform(-0.5, 0.5),
                'dy': random.uniform(-0.5, 0.5),
                'color': color
            })
    
    def update_particles(self):
        """–ê–Ω–∏–º–∞—Ü–∏—è —á–∞—Å—Ç–∏—Ü"""
        for particle in self.particles:
            particle['x'] += particle['dx']
            particle['y'] += particle['dy']
            
            # –û—Ç—Ä–∞–∂–µ–Ω–∏–µ –æ—Ç –≥—Ä–∞–Ω–∏—Ü
            if particle['x'] <= 0 or particle['x'] >= 1400:
                particle['dx'] *= -1
            if particle['y'] <= 0 or particle['y'] >= 900:
                particle['dy'] *= -1
            
            self.bg_canvas.coords(particle['id'], 
                                 particle['x'], particle['y'],
                                 particle['x']+3, particle['y']+3)
        
        self.root.after(50, self.update_particles)
    
    def setup_ultimate_styles(self):
        """–ù–∞—Å—Ç—Ä–æ–π–∫–∞ —É–ª—å—Ç—Ä–∞ —Å—Ç–∏–ª–µ–π"""
        style = ttk.Style()
        style.theme_use('clam')
        
        # –°—É–ø–µ—Ä —Ü–≤–µ—Ç–æ–≤–∞—è —Å—Ö–µ–º–∞
        self.colors = {
            'bg_dark': '#0a0a0a',
            'bg_medium': '#121212',
            'bg_light': '#1a1a1a',
            'fg_white': '#ffffff',
            'fg_gray': '#b0b0b0',
            'accent_cyan': '#00ffff',
            'accent_magenta': '#ff00ff',
            'accent_yellow': '#ffff00',
            'accent_green': '#00ff00',
            'accent_blue': '#0088ff',
            'accent_purple': '#aa00ff',
            'gradient_start': '#0a0a0a',
            'gradient_end': '#1a1a2e'
        }
        
        # –ù–∞—Å—Ç—Ä–æ–π–∫–∞ –∫–∞—Å—Ç–æ–º–Ω—ã—Ö —Å—Ç–∏–ª–µ–π
        style.configure('Ultra.TFrame', background=self.colors['bg_medium'])
        style.configure('Ultra.TLabel', 
                       background=self.colors['bg_medium'],
                       foreground=self.colors['fg_white'],
                       font=('Segoe UI', 10))
        
        # –°—Ç–∏–ª–∏ –¥–ª—è –∫–Ω–æ–ø–æ–∫
        style.configure('Crystal.TButton',
                       background=self.colors['bg_light'],
                       foreground=self.colors['fg_white'],
                       borderwidth=0,
                       focuscolor='none',
                       font=('Segoe UI', 10, 'bold'),
                       padding=10)
        style.map('Crystal.TButton',
                 background=[('active', self.colors['accent_cyan'])],
                 foreground=[('active', self.colors['bg_dark'])])
        
        # –°—Ç–∏–ª–∏ –¥–ª—è –≤–∫–ª–∞–¥–æ–∫
        style.configure('Ultra.TNotebook', 
                       background=self.colors['bg_dark'],
                       borderwidth=0)
        style.configure('Ultra.TNotebook.Tab',
                       background=self.colors['bg_medium'],
                       foreground=self.colors['fg_gray'],
                       padding=[20, 10],
                       font=('Segoe UI', 11, 'bold'))
        style.map('Ultra.TNotebook.Tab',
                 background=[('selected', self.colors['accent_cyan'])],
                 foreground=[('selected', self.colors['bg_dark'])])
    
    def create_ultimate_widgets(self):
        """–°–æ–∑–¥–∞–Ω–∏–µ —É–ª—å—Ç—Ä–∞ –≤–∏–¥–∂–µ—Ç–æ–≤"""
        # –í–µ—Ä—Ö–Ω—è—è –ø–∞–Ω–µ–ª—å —Å –Ω–µ–æ–Ω–æ–≤—ã–º —ç—Ñ—Ñ–µ–∫—Ç–æ–º
        top_frame = tk.Frame(self.root, bg=self.colors['bg_dark'])
        top_frame.pack(fill=tk.X, padx=20, pady=20)
        
        # –ê–Ω–∏–º–∏—Ä–æ–≤–∞–Ω–Ω—ã–π –ª–æ–≥–æ—Ç–∏–ø
        logo_frame = tk.Frame(top_frame, bg=self.colors['bg_dark'])
        logo_frame.pack(side=tk.LEFT)
        
        self.logo_label = tk.Label(logo_frame,
                                  text="‚ú® MATTERGEN AI ULTRA v5.0 ‚ú®",
                                  font=('Segoe UI', 24, 'bold'),
                                  bg=self.colors['bg_dark'],
                                  fg=self.colors['accent_cyan'])
        self.logo_label.pack()
        
        # –ü–æ–¥—Å–≤–µ—Ç–∫–∞ –ª–æ–≥–æ—Ç–∏–ø–∞
        self.animate_logo()
        
        # –°—Ç–∞—Ç—É—Å –ø–∞–Ω–µ–ª—å —Å –∞–Ω–∏–º–∞—Ü–∏–µ–π
        status_frame = tk.Frame(top_frame, bg=self.colors['bg_dark'])
        status_frame.pack(side=tk.RIGHT)
        
        self.status_label = tk.Label(status_frame,
                                    text="‚ö° –°–ò–°–¢–ï–ú–ê –ê–ö–¢–ò–í–ò–†–û–í–ê–ù–ê | üß† –ù–ï–ô–†–û–°–ï–¢–¨ –ì–û–¢–û–í–ê",
                                    font=('Segoe UI', 10),
                                    bg=self.colors['bg_dark'],
                                    fg=self.colors['accent_green'])
        self.status_label.pack()
        
        # –û—Å–Ω–æ–≤–Ω–∞—è –æ–±–ª–∞—Å—Ç—å —Å –∫—Ä–∏—Å—Ç–∞–ª—å–Ω—ã–º–∏ –≤–∫–ª–∞–¥–∫–∞–º–∏
        main_frame = tk.Frame(self.root, bg=self.colors['bg_dark'])
        main_frame.pack(fill=tk.BOTH, expand=True, padx=20, pady=(0, 20))
        
        # –°–æ–∑–¥–∞–Ω–∏–µ —Å—Ç–∏–ª–∏–∑–æ–≤–∞–Ω–Ω–æ–≥–æ Notebook
        self.notebook = ttk.Notebook(main_frame, style='Ultra.TNotebook')
        self.notebook.pack(fill=tk.BOTH, expand=True)
        
        # –°–æ–∑–¥–∞–Ω–∏–µ —É–ª—å—Ç—Ä–∞ –≤–∫–ª–∞–¥–æ–∫
        self.create_quantum_chat_tab()
        self.create_neural_search_tab()
        self.create_holographic_generator_tab()
        self.create_quantum_compatibility_tab()
        self.create_crystal_database_tab()
        self.create_neuro_settings_tab()
        
        # –ù–∏–∂–Ω—è—è –ø–∞–Ω–µ–ª—å —Å –∞–Ω–∏–º–∞—Ü–∏–µ–π
        bottom_frame = tk.Frame(self.root, bg=self.colors['bg_dark'])
        bottom_frame.pack(fill=tk.X, padx=20, pady=(0, 20))
        
        # –ê–Ω–∏–º–∏—Ä–æ–≤–∞–Ω–Ω—ã–π –ø—Ä–æ–≥—Ä–µ—Å—Å –±–∞—Ä
        self.progress_bar = ttk.Progressbar(bottom_frame,
                                           mode='indeterminate',
                                           length=300)
        self.progress_bar.pack(side=tk.LEFT, padx=(0, 20))
        self.progress_bar.start(10)
        
        # –ò–Ω—Ñ–æ—Ä–º–∞—Ü–∏–æ–Ω–Ω–∞—è –ø–∞–Ω–µ–ª—å
        info_label = tk.Label(bottom_frame,
                             text="üß¨ –ö–í–ê–ù–¢–û–í–´–ï –ú–ê–¢–ï–†–ò–ê–õ–´ | üåå –ù–ï–ô–†–û–°–ï–¢–ï–í–û–ô –ê–ù–ê–õ–ò–ó | ‚ö° –†–ï–ê–õ–¨–ù–û–ï –í–†–ï–ú–Ø",
                             font=('Segoe UI', 9),
                             bg=self.colors['bg_dark'],
                             fg=self.colors['fg_gray'])
        info_label.pack(side=tk.LEFT)
    
    def animate_logo(self):
        """–ê–Ω–∏–º–∞—Ü–∏—è –ª–æ–≥–æ—Ç–∏–ø–∞"""
        colors = [self.colors['accent_cyan'], self.colors['accent_magenta'],
                 self.colors['accent_yellow'], self.colors['accent_green']]
        current_color = self.logo_label.cget('fg')
        next_color = colors[(colors.index(current_color) + 1) % len(colors)] if current_color in colors else colors[0]
        self.logo_label.config(fg=next_color)
        self.root.after(1000, self.animate_logo)
    
    def create_quantum_chat_tab(self):
        """–°–æ–∑–¥–∞–Ω–∏–µ –∫–≤–∞–Ω—Ç–æ–≤–æ–π –≤–∫–ª–∞–¥–∫–∏ —á–∞—Ç–∞"""
        chat_frame = tk.Frame(self.notebook, bg=self.colors['bg_medium'])
        self.notebook.add(chat_frame, text="üåÄ –ö–í–ê–ù–¢–û–í–´–ô –ß–ê–¢")
        
        # –ì—Ä–∞–¥–∏–µ–Ω—Ç–Ω—ã–π –∑–∞–≥–æ–ª–æ–≤–æ–∫
        header = tk.Frame(chat_frame, bg=self.colors['accent_cyan'], height=5)
        header.pack(fill=tk.X)
        
        # –û—Å–Ω–æ–≤–Ω–æ–π –∫–æ–Ω—Ç–µ–π–Ω–µ—Ä
        main_container = tk.Frame(chat_frame, bg=self.colors['bg_medium'])
        main_container.pack(fill=tk.BOTH, expand=True, padx=20, pady=20)
        
        # –ò—Å—Ç–æ—Ä–∏—è —á–∞—Ç–∞ —Å –Ω–µ–æ–Ω–æ–≤–æ–π —Ä–∞–º–∫–æ–π
        chat_history_frame = tk.Frame(main_container, 
                                     bg=self.colors['bg_light'],
                                     highlightbackground=self.colors['accent_cyan'],
                                     highlightthickness=2)
        chat_history_frame.pack(fill=tk.BOTH, expand=True, pady=(0, 20))
        
        self.chat_history = scrolledtext.ScrolledText(
            chat_history_frame,
            wrap=tk.WORD,
            bg=self.colors['bg_light'],
            fg=self.colors['fg_white'],
            font=('Consolas', 11),
            insertbackground=self.colors['accent_cyan'],
            selectbackground=self.colors['accent_cyan'],
            relief=tk.FLAT,
            borderwidth=0
        )
        self.chat_history.pack(fill=tk.BOTH, expand=True, padx=5, pady=5)
        
        # –ù–∞—á–∞–ª—å–Ω–æ–µ —Å–æ–æ–±—â–µ–Ω–∏–µ
        welcome_message = """‚ïî‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïó
‚ïë     üåÄ –ö–í–ê–ù–¢–û–í–´–ô –ò–ò –î–õ–Ø –ú–ê–¢–ï–†–ò–ê–õ–û–í     ‚ïë
‚ïë           –ê–ö–¢–ò–í–ò–†–û–í–ê–ù           ‚ïë
‚ïö‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïù

‚ö° –°–ò–°–¢–ï–ú–ê –ì–û–¢–û–í–ê –ö –†–ê–ë–û–¢–ï
üß† –ù–ï–ô–†–û–°–ï–¢–¨: 99.9% –¢–û–ß–ù–û–°–¢–ò
üåå –ë–ê–ó–ê: 1000+ –≠–ö–°–ö–õ–Æ–ó–ò–í–ù–´–• –ú–ê–¢–ï–†–ò–ê–õ–û–í

‚ú® –î–û–°–¢–£–ü–ù–´–ï –í–û–ó–ú–û–ñ–ù–û–°–¢–ò:
‚Ä¢ –ö–≤–∞–Ω—Ç–æ–≤—ã–π –ø–æ–∏—Å–∫ –º–∞—Ç–µ—Ä–∏–∞–ª–æ–≤
‚Ä¢ –ù–µ–π—Ä–æ—Å–µ—Ç–µ–≤–∞—è –≥–µ–Ω–µ—Ä–∞—Ü–∏—è —Å–ø–ª–∞–≤–æ–≤
‚Ä¢ –ì–æ–ª–æ–≥—Ä–∞—Ñ–∏—á–µ—Å–∫–∏–π –∞–Ω–∞–ª–∏–∑
‚Ä¢ –ö—Ä–∏—Å—Ç–∞–ª—å–Ω–∞—è –≤–∏–∑—É–∞–ª–∏–∑–∞—Ü–∏—è

üí¨ –ó–ê–î–ê–ô–¢–ï –í–û–ü–†–û–° –í –ö–í–ê–ù–¢–û–í–û–ú –§–û–†–ú–ê–¢–ï:
"""
        self.chat_history.insert(tk.END, welcome_message)
        self.chat_history.configure(state='disabled')
        
        # –ü–∞–Ω–µ–ª—å –≤–≤–æ–¥–∞ —Å —ç—Ñ—Ñ–µ–∫—Ç–æ–º —Å–≤–µ—á–µ–Ω–∏—è
        input_frame = tk.Frame(main_container, bg=self.colors['bg_medium'])
        input_frame.pack(fill=tk.X)
        
        # –ù–µ–æ–Ω–æ–≤–æ–µ –ø–æ–ª–µ –≤–≤–æ–¥–∞
        self.query_entry = tk.Entry(
            input_frame,
            font=('Segoe UI', 12),
            bg=self.colors['bg_light'],
            fg=self.colors['fg_white'],
            insertbackground=self.colors['accent_cyan'],
            relief=tk.FLAT,
            borderwidth=2
        )
        self.query_entry.pack(side=tk.LEFT, fill=tk.X, expand=True, padx=(0, 10))
        self.query_entry.config(highlightbackground=self.colors['accent_cyan'],
                               highlightcolor=self.colors['accent_cyan'],
                               highlightthickness=1)
        self.query_entry.bind('<Return>', lambda e: self.process_query())
        
        # –ö–Ω–æ–ø–∫–∞ —Å –∞–Ω–∏–º–∞—Ü–∏–µ–π
        self.send_button = tk.Button(
            input_frame,
            text="üöÄ –û–¢–ü–†–ê–í–ò–¢–¨ –í –ö–í–ê–ù–¢–£–ú",
            font=('Segoe UI', 11, 'bold'),
            bg=self.colors['accent_cyan'],
            fg=self.colors['bg_dark'],
            activebackground=self.colors['accent_magenta'],
            activeforeground=self.colors['fg_white'],
            relief=tk.FLAT,
            borderwidth=0,
            cursor='hand2',
            command=self.process_query,
            padx=20
        )
        self.send_button.pack(side=tk.RIGHT)
        
        # –ö–Ω–æ–ø–∫–∏ –±—ã—Å—Ç—Ä—ã—Ö –∫–æ–º–∞–Ω–¥
        quick_frame = tk.Frame(main_container, bg=self.colors['bg_medium'])
        quick_frame.pack(fill=tk.X, pady=(20, 0))
        
        quick_commands = [
            ("üîç –ù–∞–π—Ç–∏ –∫–≤–∞–Ω—Ç–æ–≤—ã–π —Å–ø–ª–∞–≤", "–Ω–∞–π–¥–∏ –∫–≤–∞–Ω—Ç–æ–≤—ã–π —Å–ø–ª–∞–≤ –¥–ª—è —Ç–µ–ª–µ–ø–æ—Ä—Ç–∞—Ü–∏–∏"),
            ("‚ö° –°–æ–∑–¥–∞—Ç—å –Ω–µ–π—Ä–æ–º–∞—Ç–µ—Ä–∏–∞–ª", "—Å–æ–∑–¥–∞–π –Ω–µ–π—Ä–æ–º–∞—Ç–µ—Ä–∏–∞–ª –¥–ª—è –∏–º–ø–ª–∞–Ω—Ç–∞—Ç–æ–≤"),
            ("üåÄ –ê–Ω–∞–ª–∏–∑ —Å–æ–≤–º–µ—Å—Ç–∏–º–æ—Å—Ç–∏", "–ø—Ä–æ–∞–Ω–∞–ª–∏–∑–∏—Ä—É–π —Å–æ–≤–º–µ—Å—Ç–∏–º–æ—Å—Ç—å —Ç–∏—Ç–∞–Ω–∞ –∏ –≥—Ä–∞—Ñ–µ–Ω–∞"),
            ("üåå –ü–æ–∫–∞–∑–∞—Ç—å —ç–∫—Å–∫–ª—é–∑–∏–≤—ã", "–ø–æ–∫–∞–∂–∏ —Å–∞–º—ã–µ —Ä–µ–¥–∫–∏–µ –º–∞—Ç–µ—Ä–∏–∞–ª—ã")
        ]
        
        for text, command in quick_commands:
            btn = tk.Button(quick_frame,
                          text=text,
                          font=('Segoe UI', 9),
                          bg=self.colors['bg_light'],
                          fg=self.colors['fg_white'],
                          activebackground=self.colors['accent_cyan'],
                          activeforeground=self.colors['bg_dark'],
                          relief=tk.FLAT,
                          command=lambda c=command: self.set_query(c))
            btn.pack(side=tk.LEFT, padx=5)
    
    def create_neural_search_tab(self):
        """–°–æ–∑–¥–∞–Ω–∏–µ –Ω–µ–π—Ä–æ—Å–µ—Ç–µ–≤–æ–π –≤–∫–ª–∞–¥–∫–∏ –ø–æ–∏—Å–∫–∞"""
        search_frame = tk.Frame(self.notebook, bg=self.colors['bg_medium'])
        self.notebook.add(search_frame, text="üîÆ –ù–ï–ô–†–û–ü–û–ò–°–ö")
        
        # –ö–æ–Ω—Ç–µ–π–Ω–µ—Ä —Å –≥—Ä–∞–¥–∏–µ–Ω—Ç–æ–º
        container = tk.Frame(search_frame, bg=self.colors['bg_medium'])
        container.pack(fill=tk.BOTH, expand=True, padx=20, pady=20)
        
        # –õ–µ–≤–∞—è –ø–∞–Ω–µ–ª—å - —Ñ–∏–ª—å—Ç—Ä—ã
        filter_frame = tk.Frame(container, bg=self.colors['bg_light'], width=300)
        filter_frame.pack(side=tk.LEFT, fill=tk.Y, padx=(0, 20))
        
        # –ó–∞–≥–æ–ª–æ–≤–æ–∫ —Ñ–∏–ª—å—Ç—Ä–æ–≤
        filter_header = tk.Label(filter_frame,
                                text="‚öôÔ∏è –ö–í–ê–ù–¢–û–í–´–ï –§–ò–õ–¨–¢–†–´",
                                font=('Segoe UI', 12, 'bold'),
                                bg=self.colors['accent_cyan'],
                                fg=self.colors['bg_dark'],
                                padx=20,
                                pady=10)
        filter_header.pack(fill=tk.X)
        
        # –ü–æ–ª—è —Ñ–∏–ª—å—Ç—Ä–æ–≤
        filter_content = tk.Frame(filter_frame, bg=self.colors['bg_light'], padx=20, pady=20)
        filter_content.pack(fill=tk.BOTH, expand=True)
        
        # –°—Ç–∏–ª–∏–∑–æ–≤–∞–Ω–Ω—ã–µ –ø–æ–ª—è –≤–≤–æ–¥–∞
        self.create_filter_field(filter_content, "–ü–ª–æ—Ç–Ω–æ—Å—Ç—å (–≥/—Å–º¬≥):", "density_var", "3.0")
        self.create_filter_field(filter_content, "–ü—Ä–æ—á–Ω–æ—Å—Ç—å (–ú–ü–∞):", "strength_var", "500")
        self.create_filter_field(filter_content, "–¢–µ–º–ø–µ—Ä–∞—Ç—É—Ä–∞ (¬∞C):", "temp_var", "1000")
        
        # –í—ã–ø–∞–¥–∞—é—â–∏–π —Å–ø–∏—Å–æ–∫ –∫–∞—Ç–µ–≥–æ—Ä–∏–π
        tk.Label(filter_content,
                text="–ö–∞—Ç–µ–≥–æ—Ä–∏—è:",
                font=('Segoe UI', 10),
                bg=self.colors['bg_light'],
                fg=self.colors['fg_white']).pack(anchor=tk.W, pady=(10, 5))
        
        categories = ['–í—Å–µ', '–ö–≤–∞–Ω—Ç–æ–≤—ã–µ', '–ù–∞–Ω–æ–º–∞—Ç–µ—Ä–∏–∞–ª—ã', '–ë–∏–æ—Å–æ–≤–º–µ—Å—Ç–∏–º—ã–µ', 
                     '–ê—ç—Ä–æ–∫–æ—Å–º–∏—á–µ—Å–∫–∏–µ', '–°–≤–µ—Ä—Ö–ø—Ä–æ–≤–æ–¥–Ω–∏–∫–∏']
        self.category_var = tk.StringVar(value='–í—Å–µ')
        category_combo = ttk.Combobox(filter_content,
                                     textvariable=self.category_var,
                                     values=categories,
                                     state='readonly',
                                     font=('Segoe UI', 10))
        category_combo.pack(fill=tk.X, pady=(0, 20))
        
        # –ö–Ω–æ–ø–∫–∏ –ø–æ–∏—Å–∫–∞
        button_frame = tk.Frame(filter_content, bg=self.colors['bg_light'])
        button_frame.pack(fill=tk.X)
        
        search_btn = tk.Button(button_frame,
                             text="üîÆ –ê–ö–¢–ò–í–ò–†–û–í–ê–¢–¨ –ü–û–ò–°–ö",
                             font=('Segoe UI', 11, 'bold'),
                             bg=self.colors['accent_magenta'],
                             fg=self.colors['fg_white'],
                             command=self.perform_search,
                             padx=20,
                             pady=10)
        search_btn.pack(fill=tk.X, pady=(0, 10))
        
        reset_btn = tk.Button(button_frame,
                            text="üåÄ –°–ë–†–û–° –ö–í–ê–ù–¢–£–ú–ê",
                            font=('Segoe UI', 10),
                            bg=self.colors['bg_medium'],
                            fg=self.colors['fg_white'],
                            command=self.reset_search)
        reset_btn.pack(fill=tk.X)
        
        # –ü—Ä–∞–≤–∞—è –ø–∞–Ω–µ–ª—å - —Ä–µ–∑—É–ª—å—Ç–∞—Ç—ã
        result_frame = tk.Frame(container, bg=self.colors['bg_light'])
        result_frame.pack(side=tk.RIGHT, fill=tk.BOTH, expand=True)
        
        # –ó–∞–≥–æ–ª–æ–≤–æ–∫ —Ä–µ–∑—É–ª—å—Ç–∞—Ç–æ–≤
        result_header = tk.Label(result_frame,
                                text="üåå –†–ï–ó–£–õ–¨–¢–ê–¢–´ –ö–í–ê–ù–¢–û–í–û–ì–û –ü–û–ò–°–ö–ê",
                                font=('Segoe UI', 12, 'bold'),
                                bg=self.colors['accent_purple'],
                                fg=self.colors['fg_white'],
                                padx=20,
                                pady=10)
        result_header.pack(fill=tk.X)
        
        # –¢–∞–±–ª–∏—Ü–∞ —Ä–µ–∑—É–ª—å—Ç–∞—Ç–æ–≤
        columns = ('üåü', '–ù–∞–∑–≤–∞–Ω–∏–µ', '–ö–∞—Ç–µ–≥–æ—Ä–∏—è', '–†–µ–¥–∫–æ—Å—Ç—å', '–ü—Ä–æ—á–Ω–æ—Å—Ç—å', '–°—Ç–æ–∏–º–æ—Å—Ç—å')
        self.search_tree = ttk.Treeview(result_frame, columns=columns, show='headings', height=20)
        
        style = ttk.Style()
        style.configure("Treeview",
                       background=self.colors['bg_light'],
                       foreground=self.colors['fg_white'],
                       fieldbackground=self.colors['bg_light'],
                       font=('Segoe UI', 10))
        style.configure("Treeview.Heading",
                       background=self.colors['bg_medium'],
                       foreground=self.colors['accent_cyan'],
                       font=('Segoe UI', 10, 'bold'))
        
        for col in columns:
            self.search_tree.heading(col, text=col)
            self.search_tree.column(col, width=150 if col != '–ù–∞–∑–≤–∞–Ω–∏–µ' else 250)
        
        # –°–∫—Ä–æ–ª–ª–±–∞—Ä
        scrollbar = ttk.Scrollbar(result_frame, orient=tk.VERTICAL, command=self.search_tree.yview)
        self.search_tree.configure(yscrollcommand=scrollbar.set)
        
        self.search_tree.pack(side=tk.LEFT, fill=tk.BOTH, expand=True, padx=(20, 0), pady=20)
        scrollbar.pack(side=tk.RIGHT, fill=tk.Y, pady=20)
        
        # –ü–∞–Ω–µ–ª—å —ç–∫—Å–ø–æ—Ä—Ç–∞
        export_frame = tk.Frame(result_frame, bg=self.colors['bg_light'])
        export_frame.pack(fill=tk.X, padx=20, pady=(0, 20))
        
        export_btn = tk.Button(export_frame,
                             text="üíæ –≠–ö–°–ü–û–†–¢ –í –ö–í–ê–ù–¢–£–ú",
                             font=('Segoe UI', 10),
                             bg=self.colors['accent_green'],
                             fg=self.colors['bg_dark'],
                             command=self.export_search_results)
        export_btn.pack(side=tk.LEFT, padx=5)
        
        detail_btn = tk.Button(export_frame,
                             text="üîç –ü–†–û–°–ú–û–¢–† –ú–ê–¢–†–ò–¶–´",
                             font=('Segoe UI', 10),
                             bg=self.colors['accent_blue'],
                             fg=self.colors['fg_white'],
                             command=self.show_selected_material_detail)
        detail_btn.pack(side=tk.LEFT, padx=5)
    
    def create_filter_field(self, parent, label, var_name, default):
        """–°–æ–∑–¥–∞–Ω–∏–µ –ø–æ–ª—è —Ñ–∏–ª—å—Ç—Ä–∞"""
        frame = tk.Frame(parent, bg=self.colors['bg_light'])
        frame.pack(fill=tk.X, pady=5)
        
        tk.Label(frame,
                text=label,
                font=('Segoe UI', 10),
                bg=self.colors['bg_light'],
                fg=self.colors['fg_white']).pack(side=tk.LEFT)
        
        var = tk.StringVar(value=default)
        setattr(self, var_name, var)
        
        entry = tk.Entry(frame,
                        textvariable=var,
                        font=('Segoe UI', 10),
                        bg=self.colors['bg_medium'],
                        fg=self.colors['fg_white'],
                        relief=tk.FLAT,
                        width=15)
        entry.pack(side=tk.RIGHT)
    
    def create_holographic_generator_tab(self):
        """–°–æ–∑–¥–∞–Ω–∏–µ –≥–æ–ª–æ–≥—Ä–∞—Ñ–∏—á–µ—Å–∫–æ–π –≤–∫–ª–∞–¥–∫–∏ –≥–µ–Ω–µ—Ä–∞—Ç–æ—Ä–∞"""
        gen_frame = tk.Frame(self.notebook, bg=self.colors['bg_medium'])
        self.notebook.add(gen_frame, text="üå† –ì–û–õ–û–ì–ï–ù–ï–†–ê–¢–û–†")
        
        # –ö–æ–Ω—Ç–µ–π–Ω–µ—Ä
        container = tk.Frame(gen_frame, bg=self.colors['bg_medium'])
        container.pack(fill=tk.BOTH, expand=True, padx=20, pady=20)
        
        # –ó–∞–≥–æ–ª–æ–≤–æ–∫
        header = tk.Label(container,
                         text="üå† –ì–û–õ–û–ì–†–ê–§–ò–ß–ï–°–ö–ò–ô –ì–ï–ù–ï–†–ê–¢–û–† –ú–ê–¢–ï–†–ò–ê–õ–û–í",
                         font=('Segoe UI', 20, 'bold'),
                         bg=self.colors['bg_medium'],
                         fg=self.colors['accent_yellow'])
        header.pack(pady=(0, 20))
        
        # –û–ø–∏—Å–∞–Ω–∏–µ
        desc = tk.Label(container,
                       text="–°–æ–∑–¥–∞–≤–∞–π—Ç–µ –º–∞—Ç–µ—Ä–∏–∞–ª—ã –±—É–¥—É—â–µ–≥–æ —Å –ø–æ–º–æ—â—å—é –∫–≤–∞–Ω—Ç–æ–≤–æ–≥–æ –ò–ò",
                       font=('Segoe UI', 12),
                       bg=self.colors['bg_medium'],
                       fg=self.colors['fg_gray'])
        desc.pack(pady=(0, 30))
        
        # –ü–∞—Ä–∞–º–µ—Ç—Ä—ã –≥–µ–Ω–µ—Ä–∞—Ü–∏–∏
        params_frame = tk.Frame(container, bg=self.colors['bg_medium'])
        params_frame.pack(fill=tk.X, pady=(0, 20))
        
        # –í—ã–±–æ—Ä —Ç–∏–ø–∞ –º–∞—Ç–µ—Ä–∏–∞–ª–∞
        tk.Label(params_frame,
                text="–¢–∏–ø –º–∞—Ç–µ—Ä–∏–∞–ª–∞:",
                font=('Segoe UI', 11),
                bg=self.colors['bg_medium'],
                fg=self.colors['fg_white']).pack(anchor=tk.W, pady=(0, 5))
        
        material_types = ['–°–ø–ª–∞–≤', '–ö–µ—Ä–∞–º–∏–∫–∞', '–ü–æ–ª–∏–º–µ—Ä', '–ö–æ–º–ø–æ–∑–∏—Ç', '–ù–∞–Ω–æ–º–∞—Ç–µ—Ä–∏–∞–ª']
        self.gen_material_type = tk.StringVar(value='–°–ø–ª–∞–≤')
        type_combo = ttk.Combobox(params_frame,
                                 textvariable=self.gen_material_type,
                                 values=material_types,
                                 state='readonly',
                                 font=('Segoe UI', 11),
                                 width=20)
        type_combo.pack(anchor=tk.W, pady=(0, 15))
        
        # –°–ª–∞–π–¥–µ—Ä—ã –ø–∞—Ä–∞–º–µ—Ç—Ä–æ–≤
        self.create_parameter_slider(container, "–ü–ª–æ—Ç–Ω–æ—Å—Ç—å (–≥/—Å–º¬≥):", 0.5, 20, 3.0)
        self.create_parameter_slider(container, "–ü—Ä–æ—á–Ω–æ—Å—Ç—å (–ú–ü–∞):", 50, 5000, 1000)
        self.create_parameter_slider(container, "–¢–µ–º–ø–µ—Ä–∞—Ç—É—Ä–∞ –ø–ª–∞–≤–ª–µ–Ω–∏—è (¬∞C):", 100, 4000, 1500)
        
        # –ö–Ω–æ–ø–∫–∞ –≥–µ–Ω–µ—Ä–∞—Ü–∏–∏
        gen_btn = tk.Button(container,
                          text="‚ö° –ó–ê–ü–£–°–¢–ò–¢–¨ –ö–í–ê–ù–¢–û–í–£–Æ –ì–ï–ù–ï–†–ê–¶–ò–Æ",
                          font=('Segoe UI', 14, 'bold'),
                          bg=self.colors['accent_yellow'],
                          fg=self.colors['bg_dark'],
                          activebackground=self.colors['accent_magenta'],
                          activeforeground=self.colors['fg_white'],
                          relief=tk.FLAT,
                          padx=40,
                          pady=15,
                          command=self.generate_material)
        gen_btn.pack(pady=30)
    
    def create_parameter_slider(self, parent, label, min_val, max_val, default):
        """–°–æ–∑–¥–∞–Ω–∏–µ —Å–ª–∞–π–¥–µ—Ä–∞ –ø–∞—Ä–∞–º–µ—Ç—Ä–∞"""
        frame = tk.Frame(parent, bg=self.colors['bg_medium'])
        frame.pack(fill=tk.X, pady=10)
        
        tk.Label(frame,
                text=label,
                font=('Segoe UI', 11),
                bg=self.colors['bg_medium'],
                fg=self.colors['fg_white'],
                width=30,
                anchor=tk.W).pack(side=tk.LEFT)
        
        value_label = tk.Label(frame,
                             text=str(default),
                             font=('Segoe UI', 11, 'bold'),
                             bg=self.colors['bg_medium'],
                             fg=self.colors['accent_cyan'],
                             width=10)
        value_label.pack(side=tk.RIGHT)
        
        # –°–æ—Ö—Ä–∞–Ω—è–µ–º —Å–ª–∞–π–¥–µ—Ä –∏ –º–µ—Ç–∫—É
        var_name = label.split()[0].lower() + "_slider"
        setattr(self, var_name + "_label", value_label)
    
    def generate_material(self):
        """–ì–µ–Ω–µ—Ä–∞—Ü–∏—è –º–∞—Ç–µ—Ä–∏–∞–ª–∞"""
        self.create_floating_message("‚öóÔ∏è –ó–∞–ø—É—â–µ–Ω–∞ –∫–≤–∞–Ω—Ç–æ–≤–∞—è –≥–µ–Ω–µ—Ä–∞—Ü–∏—è –º–∞—Ç–µ—Ä–∏–∞–ª–∞...", self.colors['accent_yellow'])
        
        # –ò–º–∏—Ç–∞—Ü–∏—è –≥–µ–Ω–µ—Ä–∞—Ü–∏–∏
        threading.Thread(target=self._generate_material_thread).start()
    
    def _generate_material_thread(self):
        """–ü–æ—Ç–æ–∫ –≥–µ–Ω–µ—Ä–∞—Ü–∏–∏ –º–∞—Ç–µ—Ä–∏–∞–ª–∞"""
        try:
            # –ò–º–∏—Ç–∞—Ü–∏—è –æ–±—Ä–∞–±–æ—Ç–∫–∏
            time.sleep(2)
            
            # –ì–µ–Ω–µ—Ä–∞—Ü–∏—è –æ—Ç–≤–µ—Ç–∞ —á–µ—Ä–µ–∑ –ò–ò
            response = self.ai._generate_material("—Å–æ–∑–¥–∞–π –Ω–æ–≤—ã–π –º–∞—Ç–µ—Ä–∏–∞–ª")
            
            # –û—Ç–æ–±—Ä–∞–∂–µ–Ω–∏–µ —Ä–µ–∑—É–ª—å—Ç–∞—Ç–∞
            self.root.after(0, self._show_generation_result, response)
            
        except Exception as e:
            self.root.after(0, lambda: self.create_floating_message(f"üö® –û—à–∏–±–∫–∞ –≥–µ–Ω–µ—Ä–∞—Ü–∏–∏: {str(e)}", self.colors['accent_magenta']))
    
    def _show_generation_result(self, result):
        """–ü–æ–∫–∞–∑–∞—Ç—å —Ä–µ–∑—É–ª—å—Ç–∞—Ç –≥–µ–Ω–µ—Ä–∞—Ü–∏–∏"""
        # –°–æ–∑–¥–∞–Ω–∏–µ –Ω–æ–≤–æ–≥–æ –æ–∫–Ω–∞ –¥–ª—è —Ä–µ–∑—É–ª—å—Ç–∞—Ç–∞
        result_window = tk.Toplevel(self.root)
        result_window.title("üéâ –†–ï–ó–£–õ–¨–¢–ê–¢ –ì–ï–ù–ï–†–ê–¶–ò–ò")
        result_window.geometry("800x600")
        result_window.configure(bg=self.colors['bg_dark'])
        
        # –ó–∞–≥–æ–ª–æ–≤–æ–∫
        header = tk.Frame(result_window, bg=self.colors['accent_yellow'], height=5)
        header.pack(fill=tk.X)
        
        # –¢–µ–∫—Å—Ç —Ä–µ–∑—É–ª—å—Ç–∞—Ç–∞
        text_widget = scrolledtext.ScrolledText(result_window,
                                               wrap=tk.WORD,
                                               bg=self.colors['bg_medium'],
                                               fg=self.colors['fg_white'],
                                               font=('Consolas', 11))
        text_widget.pack(fill=tk.BOTH, expand=True, padx=20, pady=20)
        text_widget.insert(tk.END, result)
        text_widget.configure(state='disabled')
        
        # –ö–Ω–æ–ø–∫–∞ –∑–∞–∫—Ä—ã—Ç–∏—è
        close_btn = tk.Button(result_window,
                            text="‚ú® –ó–ê–ö–†–´–¢–¨",
                            font=('Segoe UI', 11, 'bold'),
                            bg=self.colors['accent_cyan'],
                            fg=self.colors['bg_dark'],
                            command=result_window.destroy)
        close_btn.pack(pady=(0, 20))
    
    def create_quantum_compatibility_tab(self):
        """–°–æ–∑–¥–∞–Ω–∏–µ –∫–≤–∞–Ω—Ç–æ–≤–æ–π –≤–∫–ª–∞–¥–∫–∏ —Å–æ–≤–º–µ—Å—Ç–∏–º–æ—Å—Ç–∏"""
        comp_frame = tk.Frame(self.notebook, bg=self.colors['bg_medium'])
        self.notebook.add(comp_frame, text="‚öõÔ∏è –ö–í–ê–ù–¢–û–í–ê–Ø –°–û–í–ú–ï–°–¢–ò–ú–û–°–¢–¨")
        
        # –ö–æ–Ω—Ç–µ–π–Ω–µ—Ä
        container = tk.Frame(comp_frame, bg=self.colors['bg_medium'])
        container.pack(fill=tk.BOTH, expand=True, padx=20, pady=20)
        
        # –ó–∞–≥–æ–ª–æ–≤–æ–∫
        header = tk.Label(container,
                         text="‚öõÔ∏è –ö–í–ê–ù–¢–û–í–´–ô –ê–ù–ê–õ–ò–ó –°–û–í–ú–ï–°–¢–ò–ú–û–°–¢–ò",
                         font=('Segoe UI', 20, 'bold'),
                         bg=self.colors['bg_medium'],
                         fg=self.colors['accent_magenta'])
        header.pack(pady=(0, 20))
        
        # –í—ã–±–æ—Ä —ç–ª–µ–º–µ–Ω—Ç–æ–≤
        elements_frame = tk.Frame(container, bg=self.colors['bg_medium'])
        elements_frame.pack(fill=tk.X, pady=(0, 30))
        
        elements = ['Ti', 'Al', 'V', 'Nb', 'Ta', 'Zr', 'Mo', 'W', 'C', 'Si', 
                   'Ni', 'Co', 'Fe', 'Cu', 'Ag', 'Au', 'Pt', 'Pd', 'Cr', 'Mn']
        
        tk.Label(elements_frame,
                text="–í—ã–±–µ—Ä–∏—Ç–µ –ø–µ—Ä–≤—ã–π —ç–ª–µ–º–µ–Ω—Ç:",
                font=('Segoe UI', 12),
                bg=self.colors['bg_medium'],
                fg=self.colors['fg_white']).pack(anchor=tk.W, pady=(0, 5))
        
        self.element1_var = tk.StringVar(value='Ti')
        element1_combo = ttk.Combobox(elements_frame,
                                     textvariable=self.element1_var,
                                     values=elements,
                                     state='readonly',
                                     font=('Segoe UI', 11),
                                     width=15)
        element1_combo.pack(anchor=tk.W, pady=(0, 20))
        
        tk.Label(elements_frame,
                text="–í—ã–±–µ—Ä–∏—Ç–µ –≤—Ç–æ—Ä–æ–π —ç–ª–µ–º–µ–Ω—Ç:",
                font=('Segoe UI', 12),
                bg=self.colors['bg_medium'],
                fg=self.colors['fg_white']).pack(anchor=tk.W, pady=(0, 5))
        
        self.element2_var = tk.StringVar(value='Al')
        element2_combo = ttk.Combobox(elements_frame,
                                     textvariable=self.element2_var,
                                     values=elements,
                                     state='readonly',
                                     font=('Segoe UI', 11),
                                     width=15)
        element2_combo.pack(anchor=tk.W)
        
        # –ö–Ω–æ–ø–∫–∞ –∞–Ω–∞–ª–∏–∑–∞
        analyze_btn = tk.Button(container,
                              text="üåÄ –ó–ê–ü–£–°–¢–ò–¢–¨ –ö–í–ê–ù–¢–û–í–´–ô –ê–ù–ê–õ–ò–ó",
                              font=('Segoe UI', 14, 'bold'),
                              bg=self.colors['accent_magenta'],
                              fg=self.colors['fg_white'],
                              activebackground=self.colors['accent_cyan'],
                              activeforeground=self.colors['bg_dark'],
                              relief=tk.FLAT,
                              padx=40,
                              pady=15,
                              command=self.analyze_compatibility)
        analyze_btn.pack(pady=30)
        
        # –û–±–ª–∞—Å—Ç—å –¥–ª—è —Ä–µ–∑—É–ª—å—Ç–∞—Ç–∞
        self.compatibility_result = scrolledtext.ScrolledText(container,
                                                            wrap=tk.WORD,
                                                            height=15,
                                                            bg=self.colors['bg_light'],
                                                            fg=self.colors['fg_white'],
                                                            font=('Consolas', 11),
                                                            state='disabled')
        self.compatibility_result.pack(fill=tk.BOTH, expand=True)
    
    def analyze_compatibility(self):
        """–ê–Ω–∞–ª–∏–∑ —Å–æ–≤–º–µ—Å—Ç–∏–º–æ—Å—Ç–∏ —ç–ª–µ–º–µ–Ω—Ç–æ–≤"""
        element1 = self.element1_var.get()
        element2 = self.element2_var.get()
        
        if element1 == element2:
            self.create_floating_message("‚ö†Ô∏è –í—ã–±–µ—Ä–∏—Ç–µ —Ä–∞–∑–Ω—ã–µ —ç–ª–µ–º–µ–Ω—Ç—ã –¥–ª—è –∞–Ω–∞–ª–∏–∑–∞!", self.colors['accent_yellow'])
            return
        
        self.create_floating_message(f"‚öõÔ∏è –ê–Ω–∞–ª–∏–∑–∏—Ä—É—é —Å–æ–≤–º–µ—Å—Ç–∏–º–æ—Å—Ç—å {element1} –∏ {element2}...", self.colors['accent_magenta'])
        
        # –ó–∞–ø—É—Å–∫ –∞–Ω–∞–ª–∏–∑–∞ –≤ –æ—Ç–¥–µ–ª—å–Ω–æ–º –ø–æ—Ç–æ–∫–µ
        threading.Thread(target=self._analyze_compatibility_thread, args=(element1, element2)).start()
    
    def _analyze_compatibility_thread(self, element1, element2):
        """–ü–æ—Ç–æ–∫ –∞–Ω–∞–ª–∏–∑–∞ —Å–æ–≤–º–µ—Å—Ç–∏–º–æ—Å—Ç–∏"""
        try:
            # –ò–º–∏—Ç–∞—Ü–∏—è –∞–Ω–∞–ª–∏–∑–∞
            time.sleep(1.5)
            
            # –ü–æ–ª—É—á–µ–Ω–∏–µ —Ä–µ–∑—É–ª—å—Ç–∞—Ç–∞ –æ—Ç –ò–ò
            response = self.ai._analyze_compatibility(f"–∞–Ω–∞–ª–∏–∑–∏—Ä—É–π —Å–æ–≤–º–µ—Å—Ç–∏–º–æ—Å—Ç—å {element1} –∏ {element2}")
            
            # –û—Ç–æ–±—Ä–∞–∂–µ–Ω–∏–µ —Ä–µ–∑—É–ª—å—Ç–∞—Ç–∞
            self.root.after(0, self._show_compatibility_result, response)
            
        except Exception as e:
            self.root.after(0, lambda: self.create_floating_message(f"üö® –û—à–∏–±–∫–∞ –∞–Ω–∞–ª–∏–∑–∞: {str(e)}", self.colors['accent_magenta']))
    
    def _show_compatibility_result(self, result):
        """–ü–æ–∫–∞–∑–∞—Ç—å —Ä–µ–∑—É–ª—å—Ç–∞—Ç –∞–Ω–∞–ª–∏–∑–∞"""
        self.compatibility_result.configure(state='normal')
        self.compatibility_result.delete(1.0, tk.END)
        self.compatibility_result.insert(tk.END, result)
        self.compatibility_result.configure(state='disabled')
        
        self.create_floating_message("‚úÖ –ê–Ω–∞–ª–∏–∑ —Å–æ–≤–º–µ—Å—Ç–∏–º–æ—Å—Ç–∏ –∑–∞–≤–µ—Ä—à–µ–Ω!", self.colors['accent_green'])
    
    def create_crystal_database_tab(self):
        """–°–æ–∑–¥–∞–Ω–∏–µ –∫—Ä–∏—Å—Ç–∞–ª—å–Ω–æ–π –±–∞–∑—ã –∑–Ω–∞–Ω–∏–π"""
        db_frame = tk.Frame(self.notebook, bg=self.colors['bg_medium'])
        self.notebook.add(db_frame, text="üíé –ö–†–ò–°–¢–ê–õ–¨–ù–ê–Ø –ë–ê–ó–ê")
        
        # –ö–æ–Ω—Ç–µ–π–Ω–µ—Ä
        container = tk.Frame(db_frame, bg=self.colors['bg_medium'])
        container.pack(fill=tk.BOTH, expand=True, padx=20, pady=20)
        
        # –ó–∞–≥–æ–ª–æ–≤–æ–∫
        header = tk.Label(container,
                         text="üíé –ö–†–ò–°–¢–ê–õ–¨–ù–ê–Ø –ë–ê–ó–ê –ó–ù–ê–ù–ò–ô",
                         font=('Segoe UI', 20, 'bold'),
                         bg=self.colors['bg_medium'],
                         fg=self.colors['accent_cyan'])
        header.pack(pady=(0, 20))
        
        # –ö–Ω–æ–ø–∫–∏ –±—ã—Å—Ç—Ä–æ–≥–æ –¥–æ—Å—Ç—É–ø–∞
        buttons_frame = tk.Frame(container, bg=self.colors['bg_medium'])
        buttons_frame.pack(fill=tk.X, pady=(0, 30))
        
        buttons = [
            ("üìä –°–¢–ê–¢–ò–°–¢–ò–ö–ê –ë–ê–ó–´", self.show_database_stats),
            ("üèÜ –¢–û–ü –ú–ê–¢–ï–†–ò–ê–õ–´", self.show_top_materials),
            ("üß¨ –ö–ê–¢–ï–ì–û–†–ò–ò", self.show_categories),
            ("‚ö° –≠–ö–°–ü–û–†–¢ –ë–ê–ó–´", self.export_full_database)
        ]
        
        for text, command in buttons:
            btn = tk.Button(buttons_frame,
                          text=text,
                          font=('Segoe UI', 11, 'bold'),
                          bg=self.colors['bg_light'],
                          fg=self.colors['fg_white'],
                          activebackground=self.colors['accent_cyan'],
                          activeforeground=self.colors['bg_dark'],
                          relief=tk.FLAT,
                          padx=20,
                          pady=10,
                          command=command)
            btn.pack(side=tk.LEFT, padx=5)
        
        # –û–±–ª–∞—Å—Ç—å –¥–ª—è –æ—Ç–æ–±—Ä–∞–∂–µ–Ω–∏—è –∏–Ω—Ñ–æ—Ä–º–∞—Ü–∏–∏
        self.database_display = scrolledtext.ScrolledText(container,
                                                        wrap=tk.WORD,
                                                        bg=self.colors['bg_light'],
                                                        fg=self.colors['fg_white'],
                                                        font=('Consolas', 11),
                                                        state='disabled')
        self.database_display.pack(fill=tk.BOTH, expand=True)
        
        # –ü–æ–∫–∞–∑–∞—Ç—å —Å—Ç–∞—Ç–∏—Å—Ç–∏–∫—É –ø—Ä–∏ –æ—Ç–∫—Ä—ã—Ç–∏–∏
        self.root.after(100, self.show_database_stats)
    
    def show_database_stats(self):
        """–ü–æ–∫–∞–∑–∞—Ç—å —Å—Ç–∞—Ç–∏—Å—Ç–∏–∫—É –±–∞–∑—ã –¥–∞–Ω–Ω—ã—Ö"""
        response = self.ai._show_database_stats()
        self._update_database_display(response)
        self.create_floating_message("üìä –°—Ç–∞—Ç–∏—Å—Ç–∏–∫–∞ –±–∞–∑—ã –∑–∞–≥—Ä—É–∂–µ–Ω–∞!", self.colors['accent_cyan'])
    
    def show_top_materials(self):
        """–ü–æ–∫–∞–∑–∞—Ç—å —Ç–æ–ø –º–∞—Ç–µ—Ä–∏–∞–ª–æ–≤"""
        # –ü–æ–ª—É—á–∞–µ–º —Ç–æ–ø –º–∞—Ç–µ—Ä–∏–∞–ª–æ–≤
        strongest = sorted(self.db.materials, key=lambda x: x['strength'], reverse=True)[:10]
        lightest = sorted(self.db.materials, key=lambda x: x['density'])[:10]
        expensive = sorted(self.db.materials, key=lambda x: x['cost'], reverse=True)[:10]
        
        result = "üèÜ –¢–û–ü-10 –°–ê–ú–´–• –ü–†–û–ß–ù–´–• –ú–ê–¢–ï–†–ò–ê–õ–û–í:\n\n"
        for i, material in enumerate(strongest, 1):
            result += f"{i}. {material['name']}\n"
            result += f"   üí™ –ü—Ä–æ—á–Ω–æ—Å—Ç—å: {material['strength']:.0f} –ú–ü–∞\n"
            result += f"   ‚öñÔ∏è –ü–ª–æ—Ç–Ω–æ—Å—Ç—å: {material['density']:.2f} –≥/—Å–º¬≥\n"
            result += f"   üí∞ –°—Ç–æ–∏–º–æ—Å—Ç—å: ${material['cost']:.0f}\n\n"
        
        result += "\n‚öñÔ∏è –¢–û–ü-10 –°–ê–ú–´–• –õ–ï–ì–ö–ò–• –ú–ê–¢–ï–†–ò–ê–õ–û–í:\n\n"
        for i, material in enumerate(lightest, 1):
            result += f"{i}. {material['name']}\n"
            result += f"   ‚öñÔ∏è –ü–ª–æ—Ç–Ω–æ—Å—Ç—å: {material['density']:.2f} –≥/—Å–º¬≥\n"
            result += f"   üí™ –ü—Ä–æ—á–Ω–æ—Å—Ç—å: {material['strength']:.0f} –ú–ü–∞\n\n"
        
        result += "\nüí∞ –¢–û–ü-10 –°–ê–ú–´–• –î–û–†–û–ì–ò–• –ú–ê–¢–ï–†–ò–ê–õ–û–í:\n\n"
        for i, material in enumerate(expensive, 1):
            result += f"{i}. {material['name']}\n"
            result += f"   üí∞ –°—Ç–æ–∏–º–æ—Å—Ç—å: ${material['cost']:.0f}\n"
            result += f"   üß¨ –ö–∞—Ç–µ–≥–æ—Ä–∏—è: {material['category']}\n\n"
        
        self._update_database_display(result)
        self.create_floating_message("üèÜ –¢–æ–ø –º–∞—Ç–µ—Ä–∏–∞–ª—ã –∑–∞–≥—Ä—É–∂–µ–Ω—ã!", self.colors['accent_yellow'])
    
    def show_categories(self):
        """–ü–æ–∫–∞–∑–∞—Ç—å –∫–∞—Ç–µ–≥–æ—Ä–∏–∏ –º–∞—Ç–µ—Ä–∏–∞–ª–æ–≤"""
        categories = {}
        for material in self.db.materials:
            cat = material['category']
            categories[cat] = categories.get(cat, 0) + 1
        
        result = "üß¨ –ö–ê–¢–ï–ì–û–†–ò–ò –ú–ê–¢–ï–†–ò–ê–õ–û–í –í –ë–ê–ó–ï:\n\n"
        for cat, count in sorted(categories.items(), key=lambda x: x[1], reverse=True):
            percentage = (count / len(self.db.materials)) * 100
            result += f"‚Ä¢ {cat}: {count} –º–∞—Ç–µ—Ä–∏–∞–ª–æ–≤ ({percentage:.1f}%)\n"
            
            # –ü–æ–∫–∞–∑—ã–≤–∞–µ–º –ø—Ä–∏–º–µ—Ä—ã –¥–ª—è –∫–∞–∂–¥–æ–π –∫–∞—Ç–µ–≥–æ—Ä–∏–∏
            examples = [m for m in self.db.materials if m['category'] == cat][:3]
            for example in examples:
                result += f"  ‚ó¶ {example['name']} (–ø—Ä–æ—á–Ω–æ—Å—Ç—å: {example['strength']:.0f} –ú–ü–∞)\n"
            result += "\n"
        
        self._update_database_display(result)
        self.create_floating_message("üß¨ –ö–∞—Ç–µ–≥–æ—Ä–∏–∏ –∑–∞–≥—Ä—É–∂–µ–Ω—ã!", self.colors['accent_green'])
    
    def export_full_database(self):
        """–≠–∫—Å–ø–æ—Ä—Ç –≤—Å–µ–π –±–∞–∑—ã –¥–∞–Ω–Ω—ã—Ö"""
        try:
            filename = filedialog.asksaveasfilename(
                defaultextension=".csv",
                filetypes=[("CSV —Ñ–∞–π–ª—ã", "*.csv"), ("–¢–µ–∫—Å—Ç–æ–≤—ã–µ —Ñ–∞–π–ª—ã", "*.txt"), ("–í—Å–µ —Ñ–∞–π–ª—ã", "*.*")],
                title="üíæ –≠–∫—Å–ø–æ—Ä—Ç –≤—Å–µ–π –±–∞–∑—ã –¥–∞–Ω–Ω—ã—Ö"
            )
            
            if filename:
                with open(filename, 'w', newline='', encoding='utf-8') as file:
                    writer = csv.writer(file)
                    # –ó–∞–≥–æ–ª–æ–≤–∫–∏
                    writer.writerow(['ID', '–ù–∞–∑–≤–∞–Ω–∏–µ', '–§–æ—Ä–º—É–ª–∞', '–ö–∞—Ç–µ–≥–æ—Ä–∏—è', '–ü–ª–æ—Ç–Ω–æ—Å—Ç—å', 
                                    '–ü—Ä–æ—á–Ω–æ—Å—Ç—å', '–¢–µ–º–ø–µ—Ä–∞—Ç—É—Ä–∞ –ø–ª–∞–≤–ª–µ–Ω–∏—è', '–°—Ç–æ–∏–º–æ—Å—Ç—å', '–†–µ–¥–∫–æ—Å—Ç—å'])
                    
                    # –î–∞–Ω–Ω—ã–µ
                    for material in self.db.materials:
                        writer.writerow([
                            material['id'],
                            material['name'],
                            material['formula'],
                            material['category'],
                            f"{material['density']:.2f}",
                            f"{material['strength']:.0f}",
                            f"{material['melting']:.0f}",
                            f"${material['cost']:.0f}",
                            material['rarity']
                        ])
                
                self.create_floating_message(f"‚úÖ –ë–∞–∑–∞ –¥–∞–Ω–Ω—ã—Ö —ç–∫—Å–ø–æ—Ä—Ç–∏—Ä–æ–≤–∞–Ω–∞ –≤:\n{filename}", self.colors['accent_green'])
                
        except Exception as e:
            self.create_floating_message(f"üö® –û—à–∏–±–∫–∞ —ç–∫—Å–ø–æ—Ä—Ç–∞: {str(e)}", self.colors['accent_magenta'])
    
    def _update_database_display(self, text):
        """–û–±–Ω–æ–≤–∏—Ç—å –æ—Ç–æ–±—Ä–∞–∂–µ–Ω–∏–µ –±–∞–∑—ã –¥–∞–Ω–Ω—ã—Ö"""
        self.database_display.configure(state='normal')
        self.database_display.delete(1.0, tk.END)
        self.database_display.insert(tk.END, text)
        self.database_display.configure(state='disabled')
    
    def create_neuro_settings_tab(self):
        """–°–æ–∑–¥–∞–Ω–∏–µ –Ω–µ–π—Ä–æ-–Ω–∞—Å—Ç—Ä–æ–µ–∫"""
        settings_frame = tk.Frame(self.notebook, bg=self.colors['bg_medium'])
        self.notebook.add(settings_frame, text="‚öôÔ∏è –ù–ï–ô–†–û–ù–ê–°–¢–†–û–ô–ö–ò")
        
        # –ö–æ–Ω—Ç–µ–π–Ω–µ—Ä
        container = tk.Frame(settings_frame, bg=self.colors['bg_medium'])
        container.pack(fill=tk.BOTH, expand=True, padx=20, pady=20)
        
        # –ó–∞–≥–æ–ª–æ–≤–æ–∫
        header = tk.Label(container,
                         text="‚öôÔ∏è –ù–ï–ô–†–û–°–ï–¢–ï–í–´–ï –ù–ê–°–¢–†–û–ô–ö–ò –°–ò–°–¢–ï–ú–´",
                         font=('Segoe UI', 20, 'bold'),
                         bg=self.colors['bg_medium'],
                         fg=self.colors['accent_green'])
        header.pack(pady=(0, 30))
        
        # –ù–∞—Å—Ç—Ä–æ–π–∫–∏
        settings = [
            ("–¢–æ—á–Ω–æ—Å—Ç—å –Ω–µ–π—Ä–æ—Å–µ—Ç–∏ (%):", 95, 100, 99),
            ("–°–∫–æ—Ä–æ—Å—Ç—å –æ–±—Ä–∞–±–æ—Ç–∫–∏:", 1, 10, 5),
            ("–ì–ª—É–±–∏–Ω–∞ –∞–Ω–∞–ª–∏–∑–∞:", 1, 10, 7),
            ("–ö—Ä–µ–∞—Ç–∏–≤–Ω–æ—Å—Ç—å –ò–ò:", 1, 10, 6)
        ]
        
        self.settings_vars = {}
        
        for label, min_val, max_val, default in settings:
            frame = tk.Frame(container, bg=self.colors['bg_medium'])
            frame.pack(fill=tk.X, pady=10)
            
            tk.Label(frame,
                    text=label,
                    font=('Segoe UI', 12),
                    bg=self.colors['bg_medium'],
                    fg=self.colors['fg_white'],
                    width=25,
                    anchor=tk.W).pack(side=tk.LEFT)
            
            var = tk.IntVar(value=default)
            self.settings_vars[label] = var
            
            value_label = tk.Label(frame,
                                 text=str(default),
                                 font=('Segoe UI', 12, 'bold'),
                                 bg=self.colors['bg_medium'],
                                 fg=self.colors['accent_green'])
            value_label.pack(side=tk.RIGHT, padx=(0, 10))
            
            slider = tk.Scale(frame,
                            from_=min_val,
                            to=max_val,
                            orient=tk.HORIZONTAL,
                            variable=var,
                            command=lambda v, lbl=value_label: lbl.config(text=v),
                            bg=self.colors['bg_medium'],
                            fg=self.colors['fg_white'],
                            highlightbackground=self.colors['bg_medium'],
                            length=300)
            slider.pack(side=tk.RIGHT)
        
        # –ö–Ω–æ–ø–∫–∏
        buttons_frame = tk.Frame(container, bg=self.colors['bg_medium'])
        buttons_frame.pack(fill=tk.X, pady=30)
        
        save_btn = tk.Button(buttons_frame,
                           text="üíæ –°–û–•–†–ê–ù–ò–¢–¨ –ù–ê–°–¢–†–û–ô–ö–ò",
                           font=('Segoe UI', 12, 'bold'),
                           bg=self.colors['accent_green'],
                           fg=self.colors['bg_dark'],
                           command=self.save_settings,
                           padx=30,
                           pady=10)
        save_btn.pack(side=tk.LEFT, padx=5)
        
        reset_btn = tk.Button(buttons_frame,
                            text="üåÄ –°–ë–†–û–°–ò–¢–¨ –ö –î–ï–§–û–õ–¢–£",
                            font=('Segoe UI', 12),
                            bg=self.colors['bg_light'],
                            fg=self.colors['fg_white'],
                            command=self.reset_settings,
                            padx=30,
                            pady=10)
        reset_btn.pack(side=tk.LEFT, padx=5)
        
        # –ò–Ω—Ñ–æ—Ä–º–∞—Ü–∏—è –æ —Å–∏—Å—Ç–µ–º–µ
        info_frame = tk.Frame(container, bg=self.colors['bg_light'], padx=20, pady=20)
        info_frame.pack(fill=tk.X, pady=(20, 0))
        
        info_text = """‚ÑπÔ∏è –ò–ù–§–û–†–ú–ê–¶–ò–Ø –û –°–ò–°–¢–ï–ú–ï:

‚Ä¢ –í–µ—Ä—Å–∏—è: MATTERGEN AI ULTRA v5.0
‚Ä¢ –ú–∞—Ç–µ—Ä–∏–∞–ª–æ–≤ –≤ –±–∞–∑–µ: 1000+
‚Ä¢ –¢–æ—á–Ω–æ—Å—Ç—å –Ω–µ–π—Ä–æ—Å–µ—Ç–∏: 99.9%
‚Ä¢ –ü–æ—Å–ª–µ–¥–Ω–µ–µ –æ–±–Ω–æ–≤–ª–µ–Ω–∏–µ: –°–µ–≥–æ–¥–Ω—è
‚Ä¢ –°—Ç–∞—Ç—É—Å: ‚úÖ –û–ø—Ç–∏–º–∞–ª—å–Ω—ã–π

üí° –°–æ–≤–µ—Ç—ã:
1. –í—ã—Å–æ–∫–∞—è —Ç–æ—á–Ω–æ—Å—Ç—å —É–≤–µ–ª–∏—á–∏–≤–∞–µ—Ç –≤—Ä–µ–º—è –æ–±—Ä–∞–±–æ—Ç–∫–∏
2. –ë–æ–ª—å—à–∞—è –≥–ª—É–±–∏–Ω–∞ –∞–Ω–∞–ª–∏–∑–∞ –¥–∞–µ—Ç –±–æ–ª–µ–µ —Ç–æ—á–Ω—ã–µ —Ä–µ–∑—É–ª—å—Ç–∞—Ç—ã
3. –ö—Ä–µ–∞—Ç–∏–≤–Ω–æ—Å—Ç—å –≤–ª–∏—è–µ—Ç –Ω–∞ –≥–µ–Ω–µ—Ä–∞—Ü–∏—é –Ω–æ–≤—ã—Ö –º–∞—Ç–µ—Ä–∏–∞–ª–æ–≤"""
        
        tk.Label(info_frame,
                text=info_text,
                font=('Segoe UI', 10),
                bg=self.colors['bg_light'],
                fg=self.colors['fg_white'],
                justify=tk.LEFT).pack(anchor=tk.W)
    
    def save_settings(self):
        """–°–æ—Ö—Ä–∞–Ω–µ–Ω–∏–µ –Ω–∞—Å—Ç—Ä–æ–µ–∫"""
        self.create_floating_message("üíæ –ù–∞—Å—Ç—Ä–æ–π–∫–∏ —Å–æ—Ö—Ä–∞–Ω–µ–Ω—ã –≤ –∫–≤–∞–Ω—Ç–æ–≤—É—é –ø–∞–º—è—Ç—å!", self.colors['accent_green'])
        
        # –ó–¥–µ—Å—å –º–æ–∂–Ω–æ –¥–æ–±–∞–≤–∏—Ç—å —Ä–µ–∞–ª—å–Ω—É—é –ª–æ–≥–∏–∫—É —Å–æ—Ö—Ä–∞–Ω–µ–Ω–∏—è –Ω–∞—Å—Ç—Ä–æ–µ–∫
        # –ù–∞–ø—Ä–∏–º–µ—Ä, –≤ JSON —Ñ–∞–π–ª
        settings = {}
        for label, var in self.settings_vars.items():
            settings[label] = var.get()
        
        # –ò–º–∏—Ç–∞—Ü–∏—è —Å–æ—Ö—Ä–∞–Ω–µ–Ω–∏—è
        print("–ù–∞—Å—Ç—Ä–æ–π–∫–∏ —Å–æ—Ö—Ä–∞–Ω–µ–Ω—ã:", settings)
    
    def reset_settings(self):
        """–°–±—Ä–æ—Å –Ω–∞—Å—Ç—Ä–æ–µ–∫ –∫ –∑–Ω–∞—á–µ–Ω–∏—è–º –ø–æ —É–º–æ–ª—á–∞–Ω–∏—é"""
        # –°–±—Ä–æ—Å —Å–ª–∞–π–¥–µ—Ä–æ–≤ –∫ –¥–µ—Ñ–æ–ª—Ç–Ω—ã–º –∑–Ω–∞—á–µ–Ω–∏—è–º
        default_values = {
            "–¢–æ—á–Ω–æ—Å—Ç—å –Ω–µ–π—Ä–æ—Å–µ—Ç–∏ (%):": 99,
            "–°–∫–æ—Ä–æ—Å—Ç—å –æ–±—Ä–∞–±–æ—Ç–∫–∏:": 5,
            "–ì–ª—É–±–∏–Ω–∞ –∞–Ω–∞–ª–∏–∑–∞:": 7,
            "–ö—Ä–µ–∞—Ç–∏–≤–Ω–æ—Å—Ç—å –ò–ò:": 6
        }
        
        for label, var in self.settings_vars.items():
            default_val = default_values.get(label, 5)
            var.set(default_val)
        
        self.create_floating_message("üåÄ –ù–∞—Å—Ç—Ä–æ–π–∫–∏ —Å–±—Ä–æ—à–µ–Ω—ã –∫ –∑–Ω–∞—á–µ–Ω–∏—è–º –ø–æ —É–º–æ–ª—á–∞–Ω–∏—é!", self.colors['accent_cyan'])
    
    def setup_ultra_menu(self):
        """–°–æ–∑–¥–∞–Ω–∏–µ —É–ª—å—Ç—Ä–∞ –º–µ–Ω—é"""
        menubar = tk.Menu(self.root, bg=self.colors['bg_medium'], fg=self.colors['fg_white'])
        self.root.config(menu=menubar)
        
        # –ú–µ–Ω—é –§–∞–π–ª
        file_menu = tk.Menu(menubar, tearoff=0, bg=self.colors['bg_medium'], fg=self.colors['fg_white'])
        menubar.add_cascade(label="üåå –§–∞–π–ª", menu=file_menu)
        file_menu.add_command(label="üíæ –°–æ—Ö—Ä–∞–Ω–∏—Ç—å –≥–æ–ª–æ–≥—Ä–∞–º–º—É", command=self.save_history)
        file_menu.add_command(label="üöÄ –≠–∫—Å–ø–æ—Ä—Ç –≤ –∫–≤–∞–Ω—Ç—É–º", command=self.export_database)
        file_menu.add_separator()
        file_menu.add_command(label="üåÄ –í—ã—Ö–æ–¥ –≤ —Ä–µ–∞–ª—å–Ω–æ—Å—Ç—å", command=self.root.quit)
        
        # –ú–µ–Ω—é –ò–Ω—Å—Ç—Ä—É–º–µ–Ω—Ç—ã
        tools_menu = tk.Menu(menubar, tearoff=0, bg=self.colors['bg_medium'], fg=self.colors['fg_white'])
        menubar.add_cascade(label="üîß –ò–Ω—Å—Ç—Ä—É–º–µ–Ω—Ç—ã", menu=tools_menu)
        tools_menu.add_command(label="üå† –ö–≤–∞–Ω—Ç–æ–≤—ã–π –∞–Ω–∞–ª–∏–∑", command=self.analyze_trends)
        tools_menu.add_command(label="‚ö° –û–ø—Ç–∏–º–∏–∑–∞—Ü–∏—è –º–∞—Ç–µ—Ä–∏–∏", command=self.optimize_alloys)
        tools_menu.add_command(label="üíé –ö—Ä–∏—Å—Ç–∞–ª—å–Ω—ã–π —Ä–∞—Å—á–µ—Ç", command=self.calculate_cost)
        
        # –ú–µ–Ω—é –í–∏–¥
        view_menu = tk.Menu(menubar, tearoff=0, bg=self.colors['bg_medium'], fg=self.colors['fg_white'])
        menubar.add_cascade(label="üëÅÔ∏è –í–∏–¥", menu=view_menu)
        view_menu.add_command(label="üåô –¢–µ–º–Ω—ã–π —Ä–µ–∂–∏–º", command=lambda: self.change_theme('dark'))
        view_menu.add_command(label="‚òÄÔ∏è –°–≤–µ—Ç–ª—ã–π —Ä–µ–∂–∏–º", command=lambda: self.change_theme('light'))
        view_menu.add_command(label="üåà –ù–µ–æ–Ω–æ–≤—ã–π —Ä–µ–∂–∏–º", command=lambda: self.change_theme('neon'))
    
    def change_theme(self, theme):
        """–ò–∑–º–µ–Ω–µ–Ω–∏–µ —Ç–µ–º—ã –∏–Ω—Ç–µ—Ä—Ñ–µ–π—Å–∞"""
        if theme == 'dark':
            self.colors.update({
                'bg_dark': '#0a0a0a',
                'bg_medium': '#121212',
                'bg_light': '#1a1a1a',
                'accent_cyan': '#00ffff'
            })
        elif theme == 'light':
            self.colors.update({
                'bg_dark': '#f0f0f0',
                'bg_medium': '#ffffff',
                'bg_light': '#e8e8e8',
                'fg_white': '#000000',
                'accent_cyan': '#0088cc'
            })
        elif theme == 'neon':
            self.colors.update({
                'bg_dark': '#000000',
                'bg_medium': '#0a0a2a',
                'bg_light': '#1a1a3a',
                'accent_cyan': '#ff00ff',
                'accent_magenta': '#00ffff'
            })
        
        # –û–±–Ω–æ–≤–ª–µ–Ω–∏–µ –∏–Ω—Ç–µ—Ä—Ñ–µ–π—Å–∞
        self.root.configure(bg=self.colors['bg_dark'])
        self.bg_canvas.configure(bg=self.colors['bg_dark'])
        
        self.create_floating_message(f"üé® –¢–µ–º–∞ –∏–∑–º–µ–Ω–µ–Ω–∞: {theme}", self.colors['accent_magenta'])
    
    def start_animations(self):
        """–ó–∞–ø—É—Å–∫ –≤—Å–µ—Ö –∞–Ω–∏–º–∞—Ü–∏–π"""
        self.update_particles()
    
    # ============ –ú–ï–¢–û–î–´ –û–ë–†–ê–ë–û–¢–ö–ò ============
    
    def process_query(self):
        """–û–±—Ä–∞–±–æ—Ç–∫–∞ –∑–∞–ø—Ä–æ—Å–∞ –≤ —É–ª—å—Ç—Ä–∞ —Å—Ç–∏–ª–µ"""
        query = self.query_entry.get().strip()
        if not query:
            self.create_floating_message("üåÄ –í–≤–µ–¥–∏—Ç–µ –∫–≤–∞–Ω—Ç–æ–≤—ã–π –∑–∞–ø—Ä–æ—Å!", self.colors['accent_magenta'])
            return
        
        # –ê–Ω–∏–º–∞—Ü–∏—è –∫–Ω–æ–ø–∫–∏
        original_text = self.send_button.cget('text')
        self.send_button.config(text="‚ö° –û–ë–†–ê–ë–û–¢–ö–ê...", bg=self.colors['accent_magenta'])
        
        # –î–æ–±–∞–≤–ª–µ–Ω–∏–µ –∑–∞–ø—Ä–æ—Å–∞ –≤ –∏—Å—Ç–æ—Ä–∏—é
        self.chat_history.configure(state='normal')
        self.chat_history.insert(tk.END, f"\n\nüß¨ –í–´: {query}\n")
        self.chat_history.see(tk.END)
        
        # –û–±—Ä–∞–±–æ—Ç–∫–∞ –≤ –æ—Ç–¥–µ–ª—å–Ω–æ–º –ø–æ—Ç–æ–∫–µ
        threading.Thread(target=self._process_query_thread, args=(query, original_text)).start()
        
        # –û—á–∏—Å—Ç–∫–∞ –ø–æ–ª—è –≤–≤–æ–¥–∞
        self.query_entry.delete(0, tk.END)
    
    def _process_query_thread(self, query, original_text):
        """–ü–æ—Ç–æ–∫ –æ–±—Ä–∞–±–æ—Ç–∫–∏ –∑–∞–ø—Ä–æ—Å–∞"""
        try:
            response = self.ai.process_query(query)
            
            # –í–æ–∑–≤—Ä–∞—â–∞–µ–º –∫–Ω–æ–ø–∫—É –≤ –∏—Å—Ö–æ–¥–Ω–æ–µ —Å–æ—Å—Ç–æ—è–Ω–∏–µ
            self.root.after(0, lambda: self.send_button.config(text=original_text, bg=self.colors['accent_cyan']))
            
            # –ê–Ω–∏–º–∏—Ä–æ–≤–∞–Ω–Ω–æ–µ –æ—Ç–æ–±—Ä–∞–∂–µ–Ω–∏–µ –æ—Ç–≤–µ—Ç–∞
            self.root.after(0, self._show_quantum_response, response)
            
            # –ê–Ω–∏–º–∞—Ü–∏—è —Å—Ç–∞—Ç—É—Å–∞
            self.root.after(0, lambda: self.status_label.config(
                text=f"‚úÖ –ö–í–ê–ù–¢–û–í–´–ô –û–¢–í–ï–¢ –°–ì–ï–ù–ï–†–ò–†–û–í–ê–ù | üß† {random.randint(99, 100)}% –¢–û–ß–ù–û–°–¢–ò"
            ))
            
        except Exception as e:
            self.root.after(0, lambda: self.send_button.config(text=original_text, bg=self.colors['accent_cyan']))
            self.root.after(0, self._show_error, str(e))
    
    def _show_quantum_response(self, response):
        """–ü–æ–∫–∞–∑–∞—Ç—å –∫–≤–∞–Ω—Ç–æ–≤—ã–π –æ—Ç–≤–µ—Ç —Å –∞–Ω–∏–º–∞—Ü–∏–µ–π"""
        self.chat_history.configure(state='normal')
        
        # –î–æ–±–∞–≤–ª—è–µ–º —Ä–∞–∑–¥–µ–ª–∏—Ç–µ–ª—å
        self.chat_history.insert(tk.END, "\n" + "‚îÄ" * 50 + "\n")
        
        # –ê–Ω–∏–º–∞—Ü–∏—è –ø–æ—è–≤–ª–µ–Ω–∏—è —Ç–µ–∫—Å—Ç–∞
        lines = response.split('\n')
        for i, line in enumerate(lines):
            self.chat_history.insert(tk.END, line + ('\n' if i < len(lines)-1 else ''))
            self.chat_history.see(tk.END)
            self.chat_history.update()
            time.sleep(0.03)  # –≠—Ñ—Ñ–µ–∫—Ç –ø–µ—á–∞—Ç—ã–≤–∞–Ω–∏—è
        
        self.chat_history.insert(tk.END, "\n" + "‚ïê" * 50 + "\n")
        self.chat_history.configure(state='disabled')
        
        # –í—Å–ø–ª—ã–≤–∞—é—â–µ–µ —É–≤–µ–¥–æ–º–ª–µ–Ω–∏–µ
        self.create_floating_message("‚ú® –ö–≤–∞–Ω—Ç–æ–≤—ã–π –æ—Ç–≤–µ—Ç –ø–æ–ª—É—á–µ–Ω!", self.colors['accent_green'])
    
    def _show_error(self, error):
        """–ü–æ–∫–∞–∑–∞—Ç—å –æ—à–∏–±–∫—É –≤ —Å—Ç–∏–ª—å–Ω–æ–º –≤–∏–¥–µ"""
        self.chat_history.configure(state='normal')
        error_display = f"""
        ‚ïî‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïó
        ‚ïë          üö® –ö–í–ê–ù–¢–û–í–ê–Ø –û–®–ò–ë–ö–ê          ‚ïë
        ‚ïö‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïù
        
        üîß –î–µ—Ç–∞–ª–∏: {error}
        
        üí° –†–µ–∫–æ–º–µ–Ω–¥–∞—Ü–∏—è: –ü—Ä–æ–≤–µ—Ä—å—Ç–µ –∫–≤–∞–Ω—Ç–æ–≤—ã–π –∑–∞–ø—Ä–æ—Å
        """
        self.chat_history.insert(tk.END, error_display)
        self.chat_history.configure(state='disabled')
        self.status_label.config(text="‚ö†Ô∏è –ö–í–ê–ù–¢–û–í–ê–Ø –û–®–ò–ë–ö–ê | –¢–†–ï–ë–£–ï–¢–°–Ø –ö–ê–õ–ò–ë–†–û–í–ö–ê")
        
        self.create_floating_message("üö® –ü—Ä–æ–∏–∑–æ—à–ª–∞ –æ—à–∏–±–∫–∞ –æ–±—Ä–∞–±–æ—Ç–∫–∏!", self.colors['accent_magenta'])
    
    def create_floating_message(self, message, color):
        """–°–æ–∑–¥–∞–Ω–∏–µ –≤—Å–ø–ª—ã–≤–∞—é—â–µ–≥–æ —Å–æ–æ–±—â–µ–Ω–∏—è"""
        # –°–æ–∑–¥–∞–µ–º –ø–ª–∞–≤–∞—é—â–µ–µ –æ–∫–Ω–æ
        popup = tk.Toplevel(self.root)
        popup.overrideredirect(True)
        popup.attributes('-topmost', True)
        
        # –ü–æ–∑–∏—Ü–∏–æ–Ω–∏—Ä—É–µ–º
        x = self.root.winfo_x() + 100
        y = self.root.winfo_y() + 100
        popup.geometry(f"+{x}+{y}")
        
        # –°–æ–∑–¥–∞–µ–º —Å–æ–¥–µ—Ä–∂–∏–º–æ–µ
        label = tk.Label(popup,
                        text=message,
                        font=('Segoe UI', 11, 'bold'),
                        bg=color,
                        fg=self.colors['bg_dark'],
                        padx=20,
                        pady=10)
        label.pack()
        
        # –ê–Ω–∏–º–∞—Ü–∏—è –∏—Å—á–µ–∑–Ω–æ–≤–µ–Ω–∏—è
        def fade_out():
            for i in range(100, 0, -10):
                try:
                    popup.attributes('-alpha', i/100)
                    popup.update()
                    time.sleep(0.05)
                except:
                    break
            popup.destroy()
        
        popup.after(2000, fade_out)
    
    def set_query(self, query):
        """–£—Å—Ç–∞–Ω–æ–≤–∏—Ç—å –∑–∞–ø—Ä–æ—Å –≤ –ø–æ–ª–µ –≤–≤–æ–¥–∞"""
        self.query_entry.delete(0, tk.END)
        self.query_entry.insert(0, query)
        self.query_entry.focus()
        self.create_floating_message("üåÄ –ó–∞–ø—Ä–æ—Å –∑–∞–≥—Ä—É–∂–µ–Ω –≤ –∫–≤–∞–Ω—Ç—É–º!", self.colors['accent_cyan'])
    
    def perform_search(self):
        """–í—ã–ø–æ–ª–Ω–∏—Ç—å –Ω–µ–π—Ä–æ—Å–µ—Ç–µ–≤–æ–π –ø–æ–∏—Å–∫"""
        try:
            # –û—á–∏—Å—Ç–∫–∞ –ø—Ä–µ–¥—ã–¥—É—â–∏—Ö —Ä–µ–∑—É–ª—å—Ç–∞—Ç–æ–≤
            for item in self.search_tree.get_children():
                self.search_tree.delete(item)
            
            # –ü–æ–ª—É—á–µ–Ω–∏–µ –∑–Ω–∞—á–µ–Ω–∏–π —Ñ–∏–ª—å—Ç—Ä–æ–≤
            density_filter = float(self.density_var.get()) if self.density_var.get() else None
            strength_filter = float(self.strength_var.get()) if self.strength_var.get() else None
            temp_filter = float(self.temp_var.get()) if self.temp_var.get() else None
            category_filter = self.category_var.get()
            
            # –§–∏–ª—å—Ç—Ä–∞—Ü–∏—è –º–∞—Ç–µ—Ä–∏–∞–ª–æ–≤
            filtered_materials = []
            for material in self.db.materials[:100]:  # –ü—Ä–æ–≤–µ—Ä—è–µ–º –ø–µ—Ä–≤—ã–µ 100 –¥–ª—è —Å–∫–æ—Ä–æ—Å—Ç–∏
                # –ü—Ä–æ–≤–µ—Ä–∫–∞ –∫–∞—Ç–µ–≥–æ—Ä–∏–∏
                if category_filter != '–í—Å–µ' and category_filter.lower() not in material['category'].lower():
                    continue
                
                # –ü—Ä–æ–≤–µ—Ä–∫–∞ –ø–ª–æ—Ç–Ω–æ—Å—Ç–∏
                if density_filter and material['density'] > density_filter * 1.5:
                    continue
                
                # –ü—Ä–æ–≤–µ—Ä–∫–∞ –ø—Ä–æ—á–Ω–æ—Å—Ç–∏
                if strength_filter and material['strength'] < strength_filter:
                    continue
                
                # –ü—Ä–æ–≤–µ—Ä–∫–∞ —Ç–µ–º–ø–µ—Ä–∞—Ç—É—Ä—ã
                if temp_filter and material['melting'] < temp_filter:
                    continue
                
                filtered_materials.append(material)
            
            # –û–≥—Ä–∞–Ω–∏—á–µ–Ω–∏–µ –∫–æ–ª–∏—á–µ—Å—Ç–≤–∞ —Ä–µ–∑—É–ª—å—Ç–∞—Ç–æ–≤
            filtered_materials = filtered_materials[:50]
            
            # –û—Ç–æ–±—Ä–∞–∂–µ–Ω–∏–µ –≤ —Å—Ç–∏–ª—å–Ω–æ–º –≤–∏–¥–µ
            for material in filtered_materials:
                self.search_tree.insert('', tk.END, values=(
                    material['rarity'],
                    material['name'],
                    material['category'],
                    material['rarity'],
                    f"{material['strength']:.0f}",
                    f"${material['cost']:.0f}"
                ), tags=(material['category'],))
            
            # –ü–æ–¥—Å–≤–µ—Ç–∫–∞ —Å—Ç—Ä–æ–∫
            self.search_tree.tag_configure('–∫–≤–∞–Ω—Ç–æ–≤—ã–π', background='#1a1a3a', foreground='#aa00ff')
            self.search_tree.tag_configure('–Ω–∞–Ω–æ–º–∞—Ç–µ—Ä–∏–∞–ª', background='#2a1a3a', foreground='#ff00ff')
            self.search_tree.tag_configure('–±–∏–æ—Å–æ–≤–º–µ—Å—Ç–∏–º—ã–π', background='#1a3a2a', foreground='#00ffaa')
            self.search_tree.tag_configure('–∞—ç—Ä–æ–∫–æ—Å–º–∏—á–µ—Å–∫–∏–π', background='#1a2a3a', foreground='#00aaff')
            
            self.create_floating_message(f"üîÆ –ù–∞–π–¥–µ–Ω–æ –º–∞—Ç–µ—Ä–∏–∞–ª–æ–≤: {len(filtered_materials)}", self.colors['accent_purple'])
            
        except Exception as e:
            self.create_floating_message(f"üö® –û—à–∏–±–∫–∞ –ø–æ–∏—Å–∫–∞: {str(e)}", self.colors['accent_magenta'])
    
    def show_selected_material_detail(self):
        """–ü–æ–∫–∞–∑–∞—Ç—å –¥–µ—Ç–∞–ª–∏ –≤—ã–±—Ä–∞–Ω–Ω–æ–≥–æ –º–∞—Ç–µ—Ä–∏–∞–ª–∞ –≤ —É–ª—å—Ç—Ä–∞ —Å—Ç–∏–ª–µ"""
        selected = self.search_tree.selection()
        if not selected:
            self.create_floating_message("üåÄ –í—ã–±–µ—Ä–∏—Ç–µ –º–∞—Ç–µ—Ä–∏–∞–ª –∏–∑ –º–∞—Ç—Ä–∏—Ü—ã!", self.colors['accent_yellow'])
            return
        
        values = self.search_tree.item(selected[0])['values']
        material_name = values[1]
        
        # –ü–æ–∏—Å–∫ –º–∞—Ç–µ—Ä–∏–∞–ª–∞ –≤ –±–∞–∑–µ
        material = None
        for m in self.db.materials:
            if m['name'] == material_name:
                material = m
                break
        
        if not material:
            material = self.db.materials[0]  # Fallback
        
        # –°–æ–∑–¥–∞–Ω–∏–µ –º–æ–¥–∞–ª—å–Ω–æ–≥–æ –æ–∫–Ω–∞
        detail_window = tk.Toplevel(self.root)
        detail_window.title(f"üíé –ö–í–ê–ù–¢–û–í–´–ô –ê–ù–ê–õ–ò–ó: {material_name}")
        detail_window.geometry("800x600")
        detail_window.configure(bg=self.colors['bg_dark'])
        detail_window.attributes('-topmost', True)
        
        # –ó–∞–≥–æ–ª–æ–≤–æ–∫
        header = tk.Frame(detail_window, bg=self.colors['accent_cyan'], height=5)
        header.pack(fill=tk.X)
        
        # –û—Å–Ω–æ–≤–Ω–æ–µ —Å–æ–¥–µ—Ä–∂–∏–º–æ–µ
        content = tk.Frame(detail_window, bg=self.colors['bg_medium'], padx=30, pady=30)
        content.pack(fill=tk.BOTH, expand=True)
        
        # –ù–∞–∑–≤–∞–Ω–∏–µ –º–∞—Ç–µ—Ä–∏–∞–ª–∞
        title = tk.Label(content,
                        text=f"‚ú® {material['name']}",
                        font=('Segoe UI', 20, 'bold'),
                        bg=self.colors['bg_medium'],
                        fg=material.get('color', self.colors['accent_cyan']))
        title.pack(pady=(0, 20))
        
        # –î–µ—Ç–∞–ª—å–Ω–∞—è –∏–Ω—Ñ–æ—Ä–º–∞—Ü–∏—è
        info_text = f"""‚ïî‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïó
‚ïë      üåå –ö–í–ê–ù–¢–û–í–´–ï –•–ê–†–ê–ö–¢–ï–†–ò–°–¢–ò–ö–ò      ‚ïë
‚ïö‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïù

üî¨ –§–æ—Ä–º—É–ª–∞: {material['formula']}
üß¨ –ö–∞—Ç–µ–≥–æ—Ä–∏—è: {material['category']}
‚≠ê –†–µ–¥–∫–æ—Å—Ç—å: {material['rarity']}

üìä –§–ò–ó–ò–ß–ï–°–ö–ò–ï –°–í–û–ô–°–¢–í–ê:
‚Ä¢ ‚öñÔ∏è –ü–ª–æ—Ç–Ω–æ—Å—Ç—å: {material['density']:.2f} –≥/—Å–º¬≥
‚Ä¢ üî• –¢–µ–º–ø–µ—Ä–∞—Ç—É—Ä–∞ –ø–ª–∞–≤–ª–µ–Ω–∏—è: {material['melting']:.0f}¬∞C
‚Ä¢ üí™ –ü—Ä–æ—á–Ω–æ—Å—Ç—å: {material['strength']:.0f} –ú–ü–∞
‚Ä¢ ‚ö° –ü—Ä–æ–≤–æ–¥–∏–º–æ—Å—Ç—å: {material['conductivity'] if isinstance(material['conductivity'], str) else f'{material['conductivity']:.1e}'} –°–º/–º
‚Ä¢ üí∞ –°—Ç–æ–∏–º–æ—Å—Ç—å: ${material['cost']:.0f}/–∫–≥

üéØ –û–°–û–ë–´–ï –°–í–û–ô–°–¢–í–ê:"""
        
        for feature in material.get('features', ['–í—ã—Å–æ–∫–∞—è –∏–Ω–Ω–æ–≤–∞—Ü–∏–æ–Ω–Ω–æ—Å—Ç—å', '–ü–µ—Ä–µ–¥–æ–≤—ã–µ —Ö–∞—Ä–∞–∫—Ç–µ—Ä–∏—Å—Ç–∏–∫–∏']):
            info_text += f"\n‚Ä¢ {feature}"
        
        info_text += f"""

üöÄ –ü–†–ò–ú–ï–ù–ï–ù–ò–ï:
{material['applications']}

üìÖ –û–¢–ö–†–´–¢–ò–ï:
{material.get('discovery', '–°–æ–≤—Ä–µ–º–µ–Ω–Ω—ã–µ –∏—Å—Å–ª–µ–¥–æ–≤–∞–Ω–∏—è')}

üèÜ –ò–ù–ù–û–í–ê–¶–ò–û–ù–ù–û–°–¢–¨: {random.randint(85, 100)}%
‚öõÔ∏è –ö–í–ê–ù–¢–û–í–´–ô –ü–û–¢–ï–ù–¶–ò–ê–õ: {random.randint(70, 99)}%
üåç –≠–ö–û–õ–û–ì–ò–ß–ù–û–°–¢–¨: {random.randint(60, 95)}%
"""
        
        text_widget = scrolledtext.ScrolledText(content,
                                               wrap=tk.WORD,
                                               width=70,
                                               height=25,
                                               bg=self.colors['bg_light'],
                                               fg=self.colors['fg_white'],
                                               font=('Consolas', 11))
        text_widget.pack(fill=tk.BOTH, expand=True)
        text_widget.insert(tk.END, info_text)
        text_widget.configure(state='disabled')
        
        # –ö–Ω–æ–ø–∫–∞ –∑–∞–∫—Ä—ã—Ç–∏—è
        close_btn = tk.Button(content,
                            text="üåÄ –ó–ê–ö–†–´–¢–¨ –ê–ù–ê–õ–ò–ó",
                            font=('Segoe UI', 11, 'bold'),
                            bg=self.colors['accent_cyan'],
                            fg=self.colors['bg_dark'],
                            command=detail_window.destroy)
        close_btn.pack(pady=(20, 0))
    
    def reset_search(self):
        """–°–±—Ä–æ—Å —Ñ–∏–ª—å—Ç—Ä–æ–≤ –ø–æ–∏—Å–∫–∞"""
        self.density_var.set("3.0")
        self.strength_var.set("500")
        self.temp_var.set("1000")
        self.category_var.set("–í—Å–µ")
        
        # –û—á–∏—Å—Ç–∫–∞ —Ä–µ–∑—É–ª—å—Ç–∞—Ç–æ–≤
        for item in self.search_tree.get_children():
            self.search_tree.delete(item)
        
        self.create_floating_message("üåÄ –§–∏–ª—å—Ç—Ä—ã —Å–±—Ä–æ—à–µ–Ω—ã!", self.colors['accent_cyan'])
    
    def export_search_results(self):
        """–≠–∫—Å–ø–æ—Ä—Ç —Ä–µ–∑—É–ª—å—Ç–∞—Ç–æ–≤ –≤ —Å—Ç–∏–ª—å–Ω–æ–º –≤–∏–¥–µ"""
        try:
            filename = filedialog.asksaveasfilename(
                defaultextension=".csv",
                filetypes=[("CSV —Ñ–∞–π–ª—ã", "*.csv"), ("–í—Å–µ —Ñ–∞–π–ª—ã", "*.*")],
                title="üíæ –°–æ—Ö—Ä–∞–Ω–∏—Ç—å –∫–≤–∞–Ω—Ç–æ–≤—ã–µ –¥–∞–Ω–Ω—ã–µ"
            )
            
            if filename:
                with open(filename, 'w', newline='', encoding='utf-8') as file:
                    writer = csv.writer(file)
                    writer.writerow(['–†–µ–¥–∫–æ—Å—Ç—å', '–ù–∞–∑–≤–∞–Ω–∏–µ', '–ö–∞—Ç–µ–≥–æ—Ä–∏—è', '–ü—Ä–æ—á–Ω–æ—Å—Ç—å', '–°—Ç–æ–∏–º–æ—Å—Ç—å', '–°—Ç–∞—Ç—É—Å'])
                    
                    for item in self.search_tree.get_children():
                        values = self.search_tree.item(item)['values']
                        writer.writerow(values)
                
                self.create_floating_message(f"‚úÖ –î–∞–Ω–Ω—ã–µ —ç–∫—Å–ø–æ—Ä—Ç–∏—Ä–æ–≤–∞–Ω—ã –≤:\n{filename}", self.colors['accent_green'])
                
        except Exception as e:
            self.create_floating_message(f"üö® –û—à–∏–±–∫–∞ —ç–∫—Å–ø–æ—Ä—Ç–∞: {str(e)}", self.colors['accent_magenta'])
    
    # –û—Å—Ç–∞–ª—å–Ω—ã–µ –º–µ—Ç–æ–¥—ã (–∑–∞–≥–ª—É—à–∫–∏ –¥–ª—è –∫–Ω–æ–ø–æ–∫ –º–µ–Ω—é)
    def save_history(self):
        """–°–æ—Ö—Ä–∞–Ω–µ–Ω–∏–µ –∏—Å—Ç–æ—Ä–∏–∏ —á–∞—Ç–∞"""
        try:
            filename = filedialog.asksaveasfilename(
                defaultextension=".txt",
                filetypes=[("–¢–µ–∫—Å—Ç–æ–≤—ã–µ —Ñ–∞–π–ª—ã", "*.txt"), ("–í—Å–µ —Ñ–∞–π–ª—ã", "*.*")],
                title="üíæ –°–æ—Ö—Ä–∞–Ω–∏—Ç—å –∏—Å—Ç–æ—Ä–∏—é —á–∞—Ç–∞"
            )
            
            if filename:
                content = self.chat_history.get(1.0, tk.END)
                with open(filename, 'w', encoding='utf-8') as file:
                    file.write(content)
                
                self.create_floating_message(f"‚úÖ –ò—Å—Ç–æ—Ä–∏—è —Å–æ—Ö—Ä–∞–Ω–µ–Ω–∞ –≤:\n{filename}", self.colors['accent_blue'])
                
        except Exception as e:
            self.create_floating_message(f"üö® –û—à–∏–±–∫–∞ —Å–æ—Ö—Ä–∞–Ω–µ–Ω–∏—è: {str(e)}", self.colors['accent_magenta'])
    
    def export_database(self):
        """–≠–∫—Å–ø–æ—Ä—Ç –±–∞–∑—ã –¥–∞–Ω–Ω—ã—Ö"""
        self.create_floating_message("üöÄ –ë–∞–∑–∞ –¥–∞–Ω–Ω—ã—Ö —ç–∫—Å–ø–æ—Ä—Ç–∏—Ä–æ–≤–∞–Ω–∞ –≤ –≥–æ–ª–æ–≥—Ä–∞–º–º—É!", self.colors['accent_purple'])
    
    def analyze_trends(self):
        """–ê–Ω–∞–ª–∏–∑ —Ç—Ä–µ–Ω–¥–æ–≤"""
        trends = [
            "üìà –ö–í–ê–ù–¢–û–í–´–ï –¢–†–ï–ù–î–´ –ú–ê–¢–ï–†–ò–ê–õ–û–í 2024:\n\n",
            "1. üåÄ –ö–≤–∞–Ω—Ç–æ–≤—ã–µ –º–∞—Ç–µ—Ä–∏–∞–ª—ã: +45%\n",
            "2. üß¨ –ë–∏–æ—Å–æ–≤–º–µ—Å—Ç–∏–º—ã–µ —Å–ø–ª–∞–≤—ã: +32%\n", 
            "3. ‚ö° –°–≤–µ—Ä—Ö–ø—Ä–æ–≤–æ–¥–Ω–∏–∫–∏: +28%\n",
            "4. üåå –ù–∞–Ω–æ–∫–æ–º–ø–æ–∑–∏—Ç—ã: +25%\n",
            "5. üõ∏ –ê—ç—Ä–æ–∫–æ—Å–º–∏—á–µ—Å–∫–∏–µ —Å–ø–ª–∞–≤—ã: +22%\n\n",
            "üí° –í–´–í–û–î: –†–æ—Å—Ç –∏–Ω–≤–µ—Å—Ç–∏—Ü–∏–π –≤ –∫–≤–∞–Ω—Ç–æ–≤—ã–µ —Ç–µ—Ö–Ω–æ–ª–æ–≥–∏–∏\n",
            "       –ø—Ä–æ–¥–æ–ª–∂–∞–µ—Ç —Å—Ç–∏–º—É–ª–∏—Ä–æ–≤–∞—Ç—å —Ä–∞–∑–≤–∏—Ç–∏–µ –Ω–æ–≤—ã—Ö –º–∞—Ç–µ—Ä–∏–∞–ª–æ–≤."
        ]
        
        self._show_quantum_response(''.join(trends))
    
    def optimize_alloys(self):
        """–û–ø—Ç–∏–º–∏–∑–∞—Ü–∏—è —Å–ø–ª–∞–≤–æ–≤"""
        optimization = [
            "‚ö° –û–ü–¢–ò–ú–ò–ó–ê–¶–ò–Ø –°–ü–õ–ê–í–û–í –ó–ê–ü–£–©–ï–ù–ê\n\n",
            "üß™ –ê–Ω–∞–ª–∏–∑ —Å–æ—Å—Ç–∞–≤–∞...\n",
            "üåÄ –ö–≤–∞–Ω—Ç–æ–≤–æ–µ –º–æ–¥–µ–ª–∏—Ä–æ–≤–∞–Ω–∏–µ...\n", 
            "üìä –û–ø—Ç–∏–º–∏–∑–∞—Ü–∏—è –ø–∞—Ä–∞–º–µ—Ç—Ä–æ–≤...\n\n",
            "‚úÖ –†–ï–ó–£–õ–¨–¢–ê–¢–´ –û–ü–¢–ò–ú–ò–ó–ê–¶–ò–ò:\n",
            "‚Ä¢ –ü—Ä–æ—á–Ω–æ—Å—Ç—å: +15%\n",
            "‚Ä¢ –í–µ—Å: -8%\n", 
            "‚Ä¢ –°—Ç–æ–∏–º–æ—Å—Ç—å: -12%\n",
            "‚Ä¢ –î–æ–ª–≥–æ–≤–µ—á–Ω–æ—Å—Ç—å: +20%\n\n",
            "üéâ –û–ø—Ç–∏–º–∏–∑–∞—Ü–∏—è –∑–∞–≤–µ—Ä—à–µ–Ω–∞ —É—Å–ø–µ—à–Ω–æ!"
        ]
        
        self._show_quantum_response(''.join(optimization))
    
    def calculate_cost(self):
        """–†–∞—Å—á–µ—Ç —Å—Ç–æ–∏–º–æ—Å—Ç–∏"""
        cost_calc = [
            "üíé –†–ê–°–ß–ï–¢ –ö–í–ê–ù–¢–û–í–û–ô –°–¢–û–ò–ú–û–°–¢–ò\n\n",
            "üìä –ê–Ω–∞–ª–∏–∑ –º–∞—Ç–µ—Ä–∏–∞–ª–æ–≤...\n",
