        quantum_materials = [
            {
                'id': 'QUANTUM-SUPER',
                'name': 'üåÄ –ö–≤–∞–Ω—Ç–æ–≤—ã–π —Å–≤–µ—Ä—Ö–ø—Ä–æ–≤–æ–¥–Ω–∏–∫',
